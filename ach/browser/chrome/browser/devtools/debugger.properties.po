#~ msgstr "Cung manok"

#~ msgid "Resume"
#~ msgstr "Dwogo"

#~ msgid "Paused"
#~ msgstr "Ocung manok"

#~ msgid "Running"
#~ msgstr "Tye katic"

#~ msgid "Local"
#~ msgstr "Pa lutedero"

#~ msgid "With block"
#~ msgstr "Ki madongo"

#~ msgid "Closure"
#~ msgstr "Agiki ne"

#~ msgid "Empty"
#~ msgstr "Twolo"
