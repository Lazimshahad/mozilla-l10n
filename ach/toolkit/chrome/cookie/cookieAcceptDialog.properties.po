#: hostColon
msgid "Host:"
msgstr "Won gang:"

#: domainColon
msgid "Domain:"
msgstr "Kut:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "Kube ma ki cono keken"

#: forAnyConnection
msgid "Any type of connection"
msgstr "Kakube mo keken"

#: expireAtEndOfSession
msgid "At end of session"
msgstr "Iyonge caa mo ni"

#: showDetails
msgid "Show Details"
msgstr "Nyut Lok iyore ki yore"

#: hideDetails
msgid "Hide Details"
msgstr "Kan Lok iyore ki yore"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "Kakube %S mito ka tero lapo wic."

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "Kakube %S mito ka tero lapo wic me aryo."

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"Kakube %S mito ka tero lapo wi intanet mukene.\n"
"In itye ki %S lapo wic intanet i kakube ma eni ni."

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "Kakube %S mito ka yubo lapo wic matye ni."
