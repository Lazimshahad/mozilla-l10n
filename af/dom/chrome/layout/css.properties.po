#: MimeNotCss
msgid ""
"The stylesheet %1$S was not loaded because its MIME type, \"%2$S\", is not "
"\"text/css\"."
msgstr ""
"Die stylvel %1$S is nie gelaai nie, omdat die MIME-soort, \"%2$S\", nie "
"\"text/css\" is nie."

#: MimeNotCssWarn
msgid ""
"The stylesheet %1$S was loaded as CSS even though its MIME type, \"%2$S\", "
"is not \"text/css\"."
msgstr ""
"Die stylvel %1$S is as CSS gelaai, al is die MIME-soort, \"%2$S\", nie "
"\"text/css\" nie."

#: PEUnexpEOF2
msgid "Unexpected end of file while searching for %1$S."
msgstr "Onverwagse einde van lêer terwyl vir %1$S gesoek."

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr "Witspasiealleen-string gegee om as reël ontleed te word."

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "Deklarasie laat val."

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "Oorgeslaan na volgende deklarasie."

#: PEUnknownProperty
msgid "Unknown property '%1$S'."
msgstr "Onbekende eienskap '%1$S'."

#: PEValueParsingError
msgid "Error in parsing value for '%1$S'."
msgstr "Fout in ontleedwaarde vir '%1$S'."

#: PEExpectEndValue
msgid "Expected end of value but found '%1$S'."
msgstr "Einde van waarde verwag, maar '%1$S' gevind."

#: PESkipAtRuleEOF
msgid "end of unknown at-rule"
msgstr "einde van onbekende by-reël"

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule '%1$S'."
msgstr "Nieherkende by-reël of kon nie by-reël '%1$S' ontleed nie."

#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "karakstel-string in @charset-reël"

#: PECharsetRuleNotString
msgid "Expected charset string but found '%1$S'."
msgstr "Karakstel-string verwag, maar '%1$S' gevind."

#: PEGatherMediaEOF
msgid "end of media list in @import or @media rule"
msgstr "einde van medialys in @import- of @media-reël"

#: PEGatherMediaNotComma
msgid "Expected ',' in media list but found '%1$S'."
msgstr "',' in medialys verwag, maar '%1$S' gevind."

#: PEGatherMediaNotIdent
msgid "Expected identifier in media list but found '%1$S'."
msgstr "Identifiseerder in medialys verwag, maar '%1$S' gevind."

#: PEImportNotURI
msgid "Expected URI in @import rule but found '%1$S'."
msgstr "URI in @import-reël verwag, maar '%1$S' gevind."

#: PEImportBadURI
msgid "Invalid URI in @import rule: '%1$S'."
msgstr "Ongeldige URI in @import-reël: '%1$S'"

#: PEImportUnexpected
msgid "Found unexpected '%1$S' within @import."
msgstr "Onverwagse '%1$S' in @import gevind."

#: PEGroupRuleEOF
msgid "end of @media or @-moz-document rule"
msgstr "einde van @media- of @-moz-document-reël"

#: PEMozDocRuleBadFunc
msgid ""
"Expected url(), url-prefix(), or domain() in @-moz-document rule but found "
"'%1$S'."
msgstr ""
"url(), url-prefix() of domain() in @-moz-document-reël verwag, maar '%1$S' "
"gevind."

#: PEMozDocRuleNotURI
msgid "Expected URI in @-moz-document rule but found '%1$S'."
msgstr "URI in @-moz-document-reël verwag, maar '%1$S' gevind."

#: PEAtNSPrefixEOF
msgid "namespace prefix in @namespace rule"
msgstr "naamspasie-voorvoegsel in @namespace-reël"

#: PEAtNSURIEOF
msgid "namespace URI in @namespace rule"
msgstr "naamspasie-URI in @namespace-reël"

#: PEAtNSUnexpected
msgid "Unexpected token within @namespace: '%1$S'."
msgstr "Onverwagse teken in @namespace: '%1$S'."

#: PESkipDeclBraceEOF
msgid "closing } of declaration block"
msgstr "eind-} van deklarasieblok"

#: PESkipRSBraceEOF
msgid "closing } of invalid rule set"
msgstr "eind-} van ongeldige reëlstel"

#: PEBadSelectorRSIgnored
msgid "Ruleset ignored due to bad selector."
msgstr "Reëlstel geïgnoreer vanweë swak selektor."

#: PESelectorListExtraEOF
msgid "',' or '{'"
msgstr "',' of '{'"

#: PESelectorListExtra
msgid "Expected ',' or '{' but found '%1$S'."
msgstr "',' of '{' verwag, maar '%1$S' gevind."

#: PESelectorGroupNoSelector
msgid "Selector expected."
msgstr "Selektor verwag."

#: PESelectorGroupExtraCombinator
msgid "Dangling combinator."
msgstr "Hangende kombinator."

#: PEClassSelEOF
msgid "class name"
msgstr "klasnaam"

#: PEClassSelNotIdent
msgid "Expected identifier for class selector but found '%1$S'."
msgstr "Identifiseerder vir klasselektor verwag, maar '%1$S' gevind."

#: PETypeSelEOF
msgid "element type"
msgstr "elementsoort"

#: PETypeSelNotType
msgid "Expected element name or '*' but found '%1$S'."
msgstr "Elementnaam of '*' verwag, maar '%1$S' gevind."

#: PEUnknownNamespacePrefix
msgid "Unknown namespace prefix '%1$S'."
msgstr "Onbekende naamspasie-voorvoegsel '%1$S'."

#: PEAttributeNameEOF
msgid "attribute name"
msgstr "attribuutnaam"

#: PEAttributeNameExpected
msgid "Expected identifier for attribute name but found '%1$S'."
msgstr "Identifiseerder vir attribuutnaam verwag, maar '%1$S' gevind."

#: PEAttributeNameOrNamespaceExpected
msgid "Expected attribute name or namespace but found '%1$S'."
msgstr "Attribuutnaam of naamspasie verwag, maar '%1$S' gevind."

#: PEAttSelNoBar
msgid "Expected '|' but found '%1$S'."
msgstr "'|' verwag, maar '%1$S' gevind."

#: PEAttSelInnerEOF
msgid "part of attribute selector"
msgstr "deel van attribuutselektor"

#: PEAttSelUnexpected
msgid "Unexpected token in attribute selector: '%1$S'."
msgstr "Onverwagse teken in attribuutselektor: '%1$S'."

#: PEAttSelValueEOF
msgid "attribute value"
msgstr "attribuutwaarde"

#: PEAttSelCloseEOF
msgid "']' to end attribute selector"
msgstr "']' moet attribuutselektor afsluit"

#: PEAttSelNoClose
msgid "Expected ']' to terminate attribute selector but found '%1$S'."
msgstr "']' verwag om attribuutselektor af te sluit, maar '%1$S' gevind."

#: PEAttSelBadValue
msgid ""
"Expected identifier or string for value in attribute selector but found "
"'%1$S'."
msgstr ""
"Identifiseerder of string vir waarde in attribuutselektor verwag, maar "
"'%1$S' gevind."

#: PEPseudoSelEOF
msgid "name of pseudo-class or pseudo-element"
msgstr "naam van pseudoklas of pseudo-element"

#: PEPseudoSelBadName
msgid ""
"Expected identifier for pseudo-class or pseudo-element but found '%1$S'."
msgstr ""
"Identifiseerder vir pseudoklas of pseudo-element verwag, maar '%1$S' gevind."

#: PEPseudoSelNonFunc
msgid ""
"Function token for non-function pseudo-class or pseudo-element, or the other "
"way around, when reading '%1$S'."
msgstr ""
"Funksieteken vir niefunksie-pseudoklas of -pseudo-element, of anders om, "
"tydens lees van '%1$S'."

#: PEPseudoSelNotPE
msgid "Expected pseudo-element but found '%1$S'."
msgstr "Pseudo-element verwag, maar '%1$S' gevind."

#: PEPseudoSelDoubleNot
msgid "Negation pseudo-class can't be negated '%1$S'."
msgstr "Ontkenning-pseudoklas kan nie '%1$S' ontken word nie."

#: PEPseudoSelPEInNot
msgid "Pseudo-elements can't be negated '%1$S'."
msgstr "Pseudo-elemente kan nie '%1$S' ontken word nie."

#: PEPseudoSelNewStyleOnly
msgid "This pseudo-element must use the \"::\" form: '%1$S'."
msgstr "Hierdie pseudo-element moet in die \"::\"-vorm '%1$S' wees."

#: PEPseudoSelTrailing
msgid ""
"Found trailing token after pseudo-element, which must be the last part of a "
"selector:  '%1$S'."
msgstr ""
"Agterste teken ná pseudo-element gevind wat die laaste deel van 'n selektor "
"moet wees:  '%1$S'."

#: PEPseudoSelMultiplePE
msgid "Extra pseudo-element '%1$S'."
msgstr "Ekstra pseudo-element '%1$S'."

#: PEPseudoSelUnknown
msgid "Unknown pseudo-class or pseudo-element '%1$S'."
msgstr "Onbekende pseudoklas of pseudo-element '%1$S'."

#: PENegationEOF
msgid "selector within negation"
msgstr "selektor binne ontkenning"

#: PENegationBadInner
msgid "Malformed simple selector as negation pseudo-class argument '%1$S'."
msgstr ""
"Wangevormde eenvoudige selektor as ontkenning-pseudoklas-argument '%1$S'."

#: PENegationNoClose
msgid "Missing closing ')' in negation pseudo-class '%1$S'."
msgstr "Vermiste eind-')' in ontkenning-pseudoklas '%1$S'."

#: PENegationBadArg
msgid "Missing argument in negation pseudo-class '%1$S'."
msgstr "Vermiste argument in ontkenning-pseudoklas '%1$S'."

#: PEPseudoClassArgEOF
msgid "argument to pseudo-class selector"
msgstr "argument aan pseudoklaskieser"

#: PEPseudoClassArgNotIdent
msgid "Expected identifier for pseudo-class parameter but found '%1$S'."
msgstr "Identifiseerder vir pseudoklas-parameter verwag, maar '%1$S' gevind."

#: PEPseudoClassArgNotNth
msgid "Expected part of argument to pseudo-class but found '%1$S'."
msgstr "Deel van pseudoklas-argument verwag, maar '%1$S' gevind."

#: PEPseudoClassNoClose
msgid "Missing closing ')' in pseudo-class, found '%1$S' instead."
msgstr "Vermiste eind-')' in pseudoklas, het '%1$S' instede gevind."

#: PEPseudoClassNoArg
msgid "Missing argument in pseudo-class '%1$S'."
msgstr "Vermiste argument in pseudoklas '%1$S'."

#: PESelectorEOF
msgid "selector"
msgstr "selector"

#: PEBadDeclBlockStart
msgid "Expected '{' to begin declaration block but found '%1$S'."
msgstr "'{' verwag om deklarasieblok te begin, maar '%1$S' gevind."

#: PEColorEOF
msgid "color"
msgstr "kleur"

#: PEColorNotColor
msgid "Expected color but found '%1$S'."
msgstr "Kleur verwag, maar '%1$S' gevind."

#: PEColorComponentEOF
msgid "color component"
msgstr "kleurkomponent"

#: PEExpectedPercent
msgid "Expected a percentage but found '%1$S'."
msgstr "'n Persentasie verwag, maar '%1$S' gevind."

#: PEExpectedInt
msgid "Expected an integer but found '%1$S'."
msgstr "'n Heeltal verwag, maar '%1$S' gevind."

#: PEColorBadRGBContents
msgid "Expected number or percentage in rgb() but found '%1$S'."
msgstr "'n Syfer of persentasie in rgb() verwag, maar '%1$S' gevind."

#: PEColorComponentBadTerm
msgid "Expected '%2$S' but found '%1$S'."
msgstr "'%2$S' verwag, maar '%1$S' gevind."

#: PEColorHueEOF
msgid "hue"
msgstr "skakering"

#: PEExpectedComma
msgid "Expected ',' but found '%1$S'."
msgstr "',' verwag, maar '%1$S' gevind."

#: PEColorSaturationEOF
msgid "saturation"
msgstr "versadiging"

#: PEColorLightnessEOF
msgid "lightness"
msgstr "ligtheid"

#: PEColorOpacityEOF
msgid "opacity in color value"
msgstr "ondeursigtigheid in kleurwaarde"

#: PEExpectedNumber
msgid "Expected a number but found '%1$S'."
msgstr "'n Syfer verwag, maar '%1$S' gevind."

#: PEExpectedCloseParen
msgid "Expected ')' but found '%1$S'."
msgstr "')' verwag, maar '%1$S' gevind."

#: PEDeclEndEOF
msgid "';' or '}' to end declaration"
msgstr "';' of '}' om deklarasie af te sluit"

#: PEParseDeclarationNoColon
msgid "Expected ':' but found '%1$S'."
msgstr "':' verwag, maar '%1$S' gevind."

#: PEParseDeclarationDeclExpected
msgid "Expected declaration but found '%1$S'."
msgstr "Deklarasie verwag, maar '%1$S' gevind."

#: PEEndOfDeclEOF
msgid "end of declaration"
msgstr "einde van deklarasie"

#: PEImportantEOF
msgid "important"
msgstr "belangrik"

#: PEExpectedImportant
msgid "Expected 'important' but found '%1$S'."
msgstr "'belangrik' verwag, maar '%1$S' gevind."

#: PEBadDeclEnd
msgid "Expected ';' to terminate declaration but found '%1$S'."
msgstr "';' verwag om deklarasie af te sluit, maar '%1$S' gevind."

#: PEBadDeclOrRuleEnd2
msgid "Expected ';' or '}' to terminate declaration but found '%1$S'."
msgstr "';' of '}' verwag om deklarasie af te sluit, maar '%1$S' gevind."

#: PEInaccessibleProperty2
msgid "Cannot specify value for internal property."
msgstr "Kan nie waarde vir interne eienskap spesifiseer nie."

#: PECommentEOF
msgid "end of comment"
msgstr "einde van opmerking"

#: SEUnterminatedString
msgid "Found unclosed string '%1$S'."
msgstr "Ongeslote string '%1$S' gevind."

#: PEFontDescExpected
msgid "Expected font descriptor but found '%1$S'."
msgstr "Fontbeskrywer verwag, maar '%1$S' gevind."

#: PEUnknownFontDesc
msgid "Unknown descriptor '%1$S' in @font-face rule."
msgstr "Onbekende beskrywer '%1$S' in @font-face-reël."

#: PEMQExpectedExpressionStart
msgid "Expected '(' to start media query expression but found '%1$S'."
msgstr "'(' aan medianavraaguitdrukking se begin verwag, maar '%1$S' gevind."

#: PEMQExpressionEOF
msgid "contents of media query expression"
msgstr "inhoud van medianavraaguitdrukking"

#: PEMQExpectedFeatureName
msgid "Expected media feature name but found '%1$S'."
msgstr "Mediakenmerknaam verwag, maar '%1$S' gevind."

#: PEMQExpectedFeatureNameEnd
msgid "Expected ':' or ')' after media feature name but found '%1$S'."
msgstr "':' of ')' ná mediakenmerknaam verwag, maar '%1$S' gevind."

#: PEMQNoMinMaxWithoutValue
msgid "Media features with min- or max- must have a value."
msgstr "Mediakenmerke met min- of maks- moet 'n waarde hê."

#: PEMQExpectedFeatureValue
msgid "Found invalid value for media feature."
msgstr "Ongeldige waarde vir mediakenmerk gevind."

#: PEBadFontBlockStart
msgid "Expected '{' to begin @font-face rule but found '%1$S'."
msgstr "'{' aan @font-face-reël se begin verwag, maar '%1$S' gevind."

#: PEBadFontBlockEnd
msgid "Expected '}' to end @font-face rule but found '%1$S'."
msgstr "'}' aan @font-face-reël se einde verwag, maar '%1$S' gevind."

#: PEAnonBoxNotAlone
msgid "Did not expect anonymous box."
msgstr "Het nie anonieme kassie verwag nie."

#~ msgid "Expected end of value for property but found '%1$S'."
#~ msgstr "Einde van waarde vir eienskap verwag, maar '%1$S' gevind."

#~ msgid "argument to :lang selector"
#~ msgstr "argument vir :lang-selektor"

#~ msgid "Missing closing ')' in lang pseudo-class '%1$S'."
#~ msgstr "Vermiste eind-')' in lang-pseudoklas '%1$S'."

#~ msgid "Attempt to use inaccessible property"
#~ msgstr "Poging om ontoeganklike eienskap te gebruik"
