#. #
#. # The following are used by the imap code to display progress/status/error messages
#. #
#. # Status - opening folder
#. ## @name IMAP_STATUS_SELECTING_MAILBOX
#. ## @loc None
#: 5000
msgid "Opening folder %S…"
msgstr "Open tans vouer %S…"

#. # Status - create folder
#. ## @name IMAP_STATUS_CREATING_MAILBOX
#. ## @loc None
#: 5001
msgid "Creating folder…"
msgstr "Skep tans vouer…"

#. # Status - deleting a folder
#. ## @name IMAP_STATUS_DELETING_MAILBOX
#. ## @loc None
#. # LOCALIZATION NOTE (Error 5002): The "%S" below should not be translated.
#. # Instead, insert "%S" in your translation where you wish to display the name
#. # of the folder being deleted.
#: 5002
msgid "Deleting folder %S…"
msgstr "Skrap tans vouer %S…"

#. # Status - renaming mailbox
#. ## @name IMAP_STATUS_RENAMING_MAILBOX
#. ## @loc None
#. # LOCALIZATION NOTE (Error 5003): The "%S" below should not be translated.
#. # Instead, insert "%S" in your translation where you wish to display the name
#. # of the folder being renamed.
#: 5003
msgid "Renaming folder %S…"
msgstr "Hernoem tans vouer %S…"

#. # Status - looking for mailboxes
#. ## @name IMAP_STATUS_LOOKING_FOR_MAILBOX
#. ## @loc None
#: 5004
msgid "Looking for folders…"
msgstr "Soek tans vir vouers…"

#. # Status - subscribing to mailbox
#. ## @name IMAP_STATUS_SUBSCRIBE_TO_MAILBOX
#. ## @loc None
#. # LOCALIZATION NOTE (Error 5005): The "%S" below should not be translated.
#. # Instead, insert "%S" in your translation where you wish to display the name
#. # of the folder being subscribed to.
#: 5005
msgid "Subscribing to folder %S…"
msgstr "Teken tans in op vouer %S…"

#. # Status - unsubscribing from mailbox
#. ## @name IMAP_STATUS_UNSUBSCRIBE_MAILBOX
#. ## @loc None
#. # LOCALIZATION NOTE (Error 5005): The "%S" below should not be translated.
#. # Instead, insert "%S" in your translation where you wish to display the name
#. # of the folder being unsubscribed from.
#: 5006
msgid "Unsubscribing from folder %S…"
msgstr "Teken tans af van vouer %S…"

#. # Status - searching imap folder
#. ## @name IMAP_STATUS_SEARCH_MAILBOX
#. ## @loc None
#: 5007
msgid "Searching folder…"
msgstr "Deursoek tans vouer…"

#. # Status - getting message info
#. ## @name IMAP_STATUS_MSG_INFO
#. ## @loc None
#: 5008
msgid "Getting message info…"
msgstr "Kry tans boodskapinfo…"

#. # Status - closing a folder
#. ## @name IMAP_STATUS_CLOSE_MAILBOX
#. ## @loc None
#: 5009
msgid "Closing folder…"
msgstr "Sluit tans vouer…"

#. # Status - compacting a folder
#. ## @name IMAP_STATUS_EXPUNGING_MAILBOX
#. ## @loc None
#: 5010
msgid "Compacting folder…"
msgstr "Kompakteer tans vouer…"

#. # Status - logging out
#. ## @name IMAP_STATUS_LOGGING_OUT
#. ## @loc None
#: 5011
msgid "Logging out…"
msgstr "Meld tans af…"

#. # Status - checking server capabilities
#. ## @name IMAP_STATUS_CHECK_COMPAT
#. ## @loc None
#: 5012
msgid "Checking mail server capabilities…"
msgstr "Kontroleer tans posbedienervermoëns…"

#. # Status - logging on
#. ## @name IMAP_STATUS_SENDING_LOGIN
#. ## @loc None
#: 5013
msgctxt "5013"
msgid "Sending login information…"
msgstr "Stuur tans aanmeldinligting…"

#. # Status - auth logon
#. ## @name IMAP_STATUS_SENDING_AUTH_LOGIN
#. ## @loc None
#: 5014
msgctxt "5014"
msgid "Sending login information…"
msgstr "Stuur tans aanmeldinligting…"

#. ## @name IMAP_DOWNLOADING_MESSAGE
#. ## @loc None
#: 5015
msgid "Downloading message…"
msgstr "Laai tans boodskap af…"

#. ## @name IMAP_GETTING_ACL_FOR_FOLDER
#. ## @loc None
#. # LOCALIZATION NOTE (Error 5029): Do not translate the word "ACL" below.
#: 5029
msgid "Getting folder ACL…"
msgstr "Kry tans vouer se toeganslys…"

#. ## @name IMAP_GETTING_SERVER_INFO
#. ## @loc None
#: 5030
msgid "Getting Server Configuration Info…"
msgstr "Kry tans bedieneropstelinfo…"

#. ## @name IMAP_GETTING_MAILBOX_INFO
#. ## @loc None
#: 5031
msgid "Getting Mailbox Configuration Info…"
msgstr "Kry tans posbusopstelinfo…"

#. ## @name IMAP_EMPTY_MIME_PART
#. ## @loc None
#: 5032
msgid "This body part will be downloaded on demand."
msgstr "Hierdie deel van die lyf sal op versoek afgelaai word."

#. ## @name IMAP_RECEIVING_MESSAGE_HEADERS_OF
#. ## @loc None
#. # LOCALIZATION NOTE (Error 5036): Do not translate the word "%1$S", "%2$lu" or "%3$lu" below.
#. # Place the word %1$S in your translation where the name of the server should appear.
#. # Place the word %2$lu where the number of the header currently being downloaded should appear.
#. # Place the word %3$lu where the number of headers should appear.
#: 5036
msgid "%S Downloading message header %lu of %lu"
msgstr "%S laai tans boodskapkopskrif %lu van %lu af"

#. ## @name IMAP_RECEIVING_MESSAGE_FLAGS_OF
#. ## @loc None
#. # LOCALIZATION NOTE (Error 5037): Do not translate the word "%1$S", "%2$lu" or "%3$lu" below.
#. # Place the word %1$S in your translation where the name of the server should appear.
#. # Place the word %2$lu where the number of the flag currently being downloaded should appear.
#. # Place the word %3$lu where the number of flags should appear.
#: 5037
msgid "%S Downloading message flag %lu of %lu"
msgstr "%S laai tans boodskapvlag %lu van %lu af"

#. ## @name IMAP_DELETING_MESSAGES
#. ## @loc None
#: 5038
msgid "Deleting messages…"
msgstr "Skrap tans boodskappe…"

#. ## @name IMAP_DELETING_MESSAGE
#. ## @loc None
#: 5039
msgid "Deleting message…"
msgstr "Skrap tans boodskap…"

#. ## @name IMAP_MOVING_MESSAGES_TO
#. ## @loc None
#. # LOCALIZATION NOTE (Error 5040): Do not translate the word "%S" below.
#. # Place the word %S in your translation where the name of the folder should appear.
#: 5040
msgid "Moving messages to %S…"
msgstr "Skuif tans boodskappe na %S…"

#. ## @name IMAP_MOVING_MESSAGE_TO
#. ## @loc None
#. # LOCALIZATION NOTE (Error 5041): Do not translate the word "%S" below.
#. # Place the word %S in your translation where the name of the folder should appear.
#: 5041
msgid "Moving message to %S…"
msgstr "Skuif tans boodskap na %S…"

#. ## @name IMAP_COPYING_MESSAGES_TO
#. ## @loc None
#. # LOCALIZATION NOTE (Error 5042): Do not translate the word "%S" below.
#. # Place the word %S in your translation where the name of the folder should appear.
#: 5042
msgid "Copying messages to %S…"
msgstr "Kopieer tans boodskappe na %S…"

#. ## @name IMAP_COPYING_MESSAGE_TO
#. ## @loc None
#. # LOCALIZATION NOTE (Error 5043): Do not translate the word "%S" below.
#. # Place the word %S in your translation where the name of the folder should appear.
#: 5043
msgid "Copying message to %S…"
msgstr "Kopieer tans boodskap na %S…"

#. ## @name IMAP_FOLDER_RECEIVING_MESSAGE_OF
#. ## @loc None
#. # LOCALIZATION NOTE (Error 5045): Do not translate the word "%S" or "%lu" below.
#. # Place the word %S in your translation where the name of the folder should appear.
#. # Place the word %lu where the number of headers should appear.
#: 5045
msgid "%S - Downloading message %lu of %lu"
msgstr "%S - laai tans boodskap %lu van %lu af"

#. ## @name IMAP_DISCOVERING_MAILBOX 
#. ## @loc None
#. # LOCALIZATION NOTE (Error 5046): Do not translate the word "%S" below.
#. # Place the word %S in your translation where the name of the folder should appear.
#: 5046
msgid "Found folder: %S"
msgstr "Vouer gevind: %S"

#. ## @name IMAP_ENTER_PASSWORD_PROMPT
#. ## @loc None
#. # LOCALIZATION NOTE (Error 5047): Do not translate the word %S below.
#. # Place the word "%S" in your translation where the email address
#. # or the username should appear
#: 5047
msgid "Enter your password for %S:"
msgstr "Tik u wagwoord vir %S in:"

#. ## @name IMAP_SERVER_NOT_IMAP4
#. ## @loc None
#. # LOCALIZATION NOTE (Error 5048): Do not translate the word "IMAP4" below.
#: 5048
msgid "Mail server %S is not an IMAP4 mail server."
msgstr "Posbediener %S is nie 'n IMAP4-posbediener nie."

#. ## @name IMAP_SERVER_SAID
#. ## @loc None
#: 5049
msgid "The current command did not succeed.  The mail server responded: "
msgstr "Die huidige bevel het nie gewerk nie.  Die posbediener se respons is: "

#. ## @name IMAP_DONE
#. ## @loc None
#: 5050
msgctxt "5050"
msgid ""
msgstr ""

#. ## @name IMAP_ENTER_PASSWORD_PROMPT_TITLE
#. ## @loc None
#: 5051
msgid "Mail Server Password Required"
msgstr "Posbedienerwagwoord verlang"

#. ## @name IMAP_UNKNOWN_HOST_ERROR
#. ## @loc None
#: 5052
msgid "Failed to connect to server %S."
msgstr "Kon nie aan bediener %S verbind nie."

#. ## @name IMAP_IMAP_CONNECTION_REFUSED_ERROR
#. ## @loc None
#: 5053
msgid "Could not connect to mail server %S; the connection was refused."
msgstr "Kon nie aan posbediener %S verbind nie; die verbinding is geweier."

#. ## @name IMAP_NET_TIMEOUT_ERROR
#. ## @loc None
#: 5054
msgid "Connection to server %S timed out."
msgstr "Verbinding aan bediener %S het uitgetel."

#. # Status - no messages to download 
#. ## @name IMAP_NO_NEW_MESSAGES
#. ## @loc None
#: 5056
msgid "There are no new messages on the server."
msgstr "Daar is nie nuwe boodskappe op die bediener nie."

#. ## @name IMAP_DEFAULT_ACCOUNT_NAME
#. ## @loc None
#: 5057
msgid "Mail for %S"
msgstr "Pos vir %S"

#. ## @name IMAP_EMPTY_TRASH_CONFIRM
#. ## @loc None
#: 5061
msgid ""
"Emptying trash will delete %S and all of the messages it contains. Do you "
"want to delete this folder?"
msgstr ""
"As u die snippermandjie leeg maak, sal %S en alle boodskappe wat dit bevat, "
"geskrap word. Wil u hierdie vouer skrap?"

#. ## @name IMAP_SPECIAL_CHAR
#. ## @loc None
#: 5065
msgid ""
"The %c character is reserved on this imap server. Please choose another "
"name. "
msgstr ""
"Die %c-karakter is op hierdie IMAP-bediener gereserveer. Kies 'n ander naam. "

#. ## @name IMAP_PERSONAL_SHARED_FOLDER_TYPE_NAME
#. ## @loc None
#: 5066
msgid "Personal Folder"
msgstr "Persoonlike vouer"

#. ## @name IMAP_PUBLIC_FOLDER_TYPE_NAME
#. ## @loc None
#: 5067
msgid "Public Folder"
msgstr "Openbare vouer"

#. ## @name IMAP_OTHER_USERS_FOLDER_TYPE_NAME
#. ## @loc None
#: 5068
msgid "Other User's Folder"
msgstr "Ander gebruiker se vouer"

#. ## @name IMAP_PERSONAL_FOLDER_TYPE_DESCRIPTION
#. ## @loc None
#: 5069
msgid "This is a personal mail folder.  It is not shared."
msgstr "Hierdie is 'n persoonlike posvouer.  Dit word nie gedeel nie."

#. ## @name IMAP_PERSONAL_SHARED_FOLDER_TYPE_DESCRIPTION
#. ## @loc None
#: 5070
msgid "This is a personal mail folder.  It has been shared."
msgstr "Hierdie is 'n persoonlike posvouer.  Dit word gedeel."

#. ## @name IMAP_PUBLIC_FOLDER_TYPE_DESCRIPTION
#. ## @loc None
#: 5071
msgid "This is a public folder."
msgstr "Hierdie is 'n openbare vouer."

#. ## @name IMAP_OTHER_USERS_FOLDER_TYPE_DESCRIPTION
#. ## @loc None
#: 5072
msgid "This is a mail folder shared by the user '%S'."
msgstr "Hierdie is 'n posvouer wat deur die gebruiker '%S' gedeel word."

#. ## @name IMAP_ACL_FULL_RIGHTS
#. ## @loc None
#: 5073
msgid "Full Control"
msgstr "Volledige beheer"

#. ## @name IMAP_ACL_LOOKUP_RIGHT
#. ## @loc None
#: 5074
msgid "Lookup"
msgstr "Opsoek"

#. ## @name IMAP_ACL_READ_RIGHT
#. ## @loc None
#: 5075
msgid "Read"
msgstr "Lees"

#. ## @name IMAP_ACL_SEEN_RIGHT
#. ## @loc None
#: 5076
msgid "Set Read/Unread State"
msgstr "Stel lees/ongelees-staat"

#. ## @name IMAP_ACL_WRITE_RIGHT
#. ## @loc None
#: 5077
msgid "Write"
msgstr "Skryf"

#. ## @name IMAP_ACL_INSERT_RIGHT
#. ## @loc None
#: 5078
msgid "Insert (Copy Into)"
msgstr "Invoeg (kopieer in)"

#. ## @name IMAP_ACL_POST_RIGHT
#. ## @loc None
#: 5079
msgid "Post"
msgstr "Pos"

#. ## @name IMAP_ACL_CREATE_RIGHT
#. ## @loc None
#: 5080
msgid "Create Subfolder"
msgstr "Skep subvouer"

#. ## @name IMAP_ACL_DELETE_RIGHT
#. ## @loc None
#: 5081
msgid "Delete Messages"
msgstr "Skrap boodskappe"

#. ## @name IMAP_ACL_ADMINISTER_RIGHT
#. ## @loc None
#: 5082
msgid "Administer Folder"
msgstr "Administreer vouer"

#. ## @name IMAP_SERVER_DOESNT_SUPPORT_ACL
#. ## @loc None
#: 5084
msgid "This server does not support shared folders."
msgstr "Hierdie bediener werk nie met gedeelde vouers nie."

#. ## @name IMAP_ACL_EXPUNGE_RIGHT
#. ## @loc None
#: 5085
msgid "Expunge"
msgstr "Deurhaal"

#. ## @name IMAP_SERVER_DISCONNECTED
#. ## @loc None
#: 5090
msgid ""
"Server %S has disconnected. The server may have gone down or there may be a "
"network problem."
msgstr ""
"Bediener %S het ontkoppel. Die bediener het moontlik omgeval, of daar is 'n "
"netwerkprobleem."

#. # LOCALIZATION NOTE (autoSubscribeText): %1$S is the imap folder.
#: 5092
msgid "Would you like to subscribe to %1$S?"
msgstr "Wil u op %1$S inteken?"

#. ## @name IMAP_SERVER_DROPPED_CONNECTION
#. ## @loc None
#: 5093
msgid ""
"Unable to connect to your IMAP server. You may have exceeded the maximum "
"number of connections to this server. If so, use the Advanced IMAP Server "
"Settings dialog to reduce the number of cached connections."
msgstr ""
"Kon nie aan u IMAP-bediener koppel nie. U het dalk die maksimum getal "
"verbindings aan hierdie bediener gemaak. Indien wel, gebruik die "
"\"Gevorderde IMAP-bediener-opstelling\"-dialoog om die getal gekaste "
"verbindings te verminder."

#. ## @name IMAP_QUOTA_STATUS_FOLDERNOTOPEN
#. ## @loc None
#: 5095
msgid "Quota information is not available because the folder is not open."
msgstr "Kwota-inligting is nie beskikbaar nie omdat die vouer nie oop is nie."

#. ## @name IMAP_QUOTA_STATUS_NOTSUPPORTED
#. ## @loc None
#: 5096
msgid "This server does not support quotas."
msgstr "Hierdie bediener werk nie met kwotas nie."

#. ## @name IMAP_QUOTA_STATUS_NOQUOTA
#. ## @loc None
#: 5097
msgid "There are no storage quotas on this folder."
msgstr "Daar is geen stoorkwotas op hierdie vouer nie."

#. # Out of memory
#. ## @name IMAP_OUT_OF_MEMORY
#. ## @loc None
#: 5100
msgid "Application is out of memory."
msgstr "Die toepassing se geheue is op."

#. ## @name IMAP_COPYING_MESSAGE_OF
#. ## @loc None
#. # LOCALIZATION NOTE (Error 5103): Do not translate the word "%S" below.
#. # Place the word %3$S in your translation where the name of the destination folder should appear.
#. # Place the word %1$S where the currently copying message should appear.
#. # Place the word %2$S where the total number of messages should appear.
#: 5103
msgid "Copying Message %1$S of %2$S to %3$S"
msgstr "Kopieer tans boodskap %1$S van %2$S na %3$S"

#. ## @name IMAP_MOVE_FOLDER_TO_TRASH
#. ## @loc None
#. # LOCALIZATION NOTE (5105): Do not translate the word %S below.
#. # "%S" is the the name of the folder.
#: 5105
msgid "Are you sure you want to delete the folder '%S'?"
msgstr "Is u seker u wil die vouer '%S' skrap?"

#. ## @name IMAP_DELETE_NO_TRASH
#. ## @loc None
#. # LOCALIZATION NOTE (5106): Do not translate the word %S below.
#. # "%S" is the the name of the folder.
#: 5106
msgid ""
"Deleting this folder is not undoable and will delete all of the messages it "
"contains, and its sub-folders. Are you sure you still want to delete the "
"folder '%S'?"
msgstr ""
"OM hierdie vouer te skrap, is nie ontdoenbaar nie, en dit sal alle "
"boodskappe skrap wat dit bevat, asook die subvouers? Is u wel seker u wil "
"die vouer '%S' skrap?"

#. ## @name IMAP_DELETE_FOLDER_DIALOG_TITLE
#. ## @loc None
#: 5107
msgid "Delete Folder"
msgstr "Skrap vouer"

#. ## @name IMAP_DELETE_FOLDER_BUTTON_LABEL
#. ## @loc None
#: 5108
msgid "&Delete Folder"
msgstr "&Skrap vouer"

#. ## @name IMAP_AUTH_CHANGE_ENCRYPT_TO_PLAIN_SSL
#. ## @loc None
#. # LOCALIZATION NOTE (5110): %S is the server hostname
#: 5110
msgid ""
"The IMAP server %S does not seem to support encrypted passwords. If you just "
"set up this account, please try changing to 'Normal password' as the "
"'Authentication method' in the 'Account Settings | Server settings'. If it "
"used to work and now suddenly fails, please contact your email administrator "
"or provider."
msgstr ""

#. ## @name IMAP_AUTH_CHANGE_PLAIN_TO_ENCRYPT
#. ## @loc None
#. # LOCALIZATION NOTE (5111): %S is the server hostname
#: 5111
msgid ""
"The IMAP server %S does not allow plaintext passwords. Please try changing "
"to 'Encrypted password' as the 'Authentication method' in the 'Account "
"Settings | Server settings'."
msgstr ""

#. ## @name IMAP_AUTH_CHANGE_ENCRYPT_TO_PLAIN_NO_SSL
#. ## @loc None
#. # LOCALIZATION NOTE (5112): %S is the server hostname
#: 5112
msgid ""
"The IMAP server %S does not seem to support encrypted passwords. If you just "
"set up the account, please try changing to 'Password, transmitted "
"insecurely' as the 'Authentication method' in the 'Account Settings | Server "
"settings'. If it used to work and now suddenly fails, this is a common "
"scenario how someone could steal your password."
msgstr ""

#. ## @name IMAP_AUTH_MECH_NOT_SUPPORTED
#. ## @loc None
#. # LOCALIZATION NOTE (5113): %S is the server hostname
#: 5113
msgid ""
"The IMAP server %S does not support the selected authentication method. "
"Please change the 'Authentication method' in the 'Account Settings | Server "
"settings'."
msgstr ""

#. ## @name IMAP_AUTH_MECH_FAILED
#. ## @loc None
#. # LOCALIZATION NOTE (5114): %S is the server hostname
#: 5114
msgid ""
"All login mechanisms for %S failed. Please check the password or change the "
"'Authentication method' in the 'Account Settings | Server settings'."
msgstr ""

#. ## @name IMAP_AUTH_GSSAPI_FAILED
#. ## @loc None
#. # LOCALIZATION NOTE (5115): %S is the server hostname
#: 5115
msgid ""
"The Kerberos/GSSAPI ticket was not accepted by the IMAP server %S. Please "
"check that you are logged in to the Kerberos/GSSAPI realm."
msgstr ""

#. ## @name IMAP_LOGIN_DISABLED
#. ## @loc None
#. # LOCALIZATION NOTE (5116): %S is the account name
#: 5116
msgid ""
"You cannot log in to %S because the server doesn't allow plaintext "
"authentication without STARTTLS or SSL/TLS. Try enabling connection security "
"or secure authentication in the account settings."
msgstr ""
"U kan nie by %S aanmeld nie want die bediener laat nie skoonteksmagtiging "
"sonder STARTTLS of SSL/TLS toe nie. Probeer verbindingsekuriteit of "
"beveiligde magtiging in die rekeninginstellings aktiveer."

#. ## @name IMAP_SERVER_COMMAND_FAILED
#. ## @loc None
#. # LOCALIZATION NOTE (5117): 
#. # Place the word %1$S in your translation where the name of the account name should appear.
#. # Place the word %2$S in your translation where the server response should appear.
#: 5117
msgid ""
"The current command did not succeed. The mail server for account %1$S "
"responded: %2$S "
msgstr ""

#. ## @name IMAP_FOLDER_COMMAND_FAILED
#. ## @loc None
#. # LOCALIZATION NOTE (5118): Do not translate the word %S below.
#. # Place the word %1$S in your translation where the name of the account should appear.
#. # Place the word %2$S in your translation where the name of the folder should appear.
#. # Place the word %3$S in your translation where the server response should appear.
#: 5118
msgid ""
"The current operation on '%2$S' did not succeed. The mail server for account "
"%1$S responded: %3$S"
msgstr ""

#. ## @name IMAP_SERVER_ALERT
#. ## @loc None
#. # LOCALIZATION NOTE (5119):
#. # Place the word %1$S in your translation where the name of the account should appear.
#. # Place the word %2$S in your translation where the alert from the server should appear.
#: 5119
msgid "Alert from account %1$S: %2$S"
msgstr ""

