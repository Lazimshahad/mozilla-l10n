msgid "Advanced JavaScript Settings"
msgstr "JavaSkript nhyehyɛe akɔenim"

#. Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger
#: window.width
msgid "37em"
msgstr "37em"

#: allowScripts.label
msgid "Allow scripts to:"
msgstr "Ma askript akwan sɛ:"

#: moveResizeWindows.label moveResizeWindows.accesskey
msgid "&Move or resize existing windows"
msgstr "&Soa-kɔ anaa san susu tokuro a ɛte ase"

#: raiseLowerWindows.label raiseLowerWindows.accesskey
msgid "&Raise or lower windows"
msgstr "&Maso anaa si atokuro bi fam"

#: disableContextMenus.label disableContextMenus.accesskey
msgid "&Disable or replace context menus"
msgstr "&Dum anaa fa biribi hyɛ kɔntɛkst menu anan"

#: hideStatusBar.label hideStatusBar.accesskey
msgid "&Hide the status bar"
msgstr "&Suma tebea baa no"

#: changeStatusBar.label changeStatusBar.accesskey
msgid "&Change status bar text"
msgstr "&Sesa tebea baa tɛkste"
