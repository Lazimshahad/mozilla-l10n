#: reloadPlugin.pre
msgctxt "reloadPlugin.pre"
msgid ""
msgstr "te sɛ ma ne hia no ma ekyiri hɔ nda hɔ"

#: reloadPlugin.middle
msgid "Reload the page"
msgstr "Bue krataafa no bio"

#: reloadPlugin.post
msgid " to try again."
msgstr "fa san sɔ hwɛ"

#. This and the other report.* strings should be as short as possible, ideally 2-3 words.
#: report.please
msgid "Send crash report"
msgstr "Bɔ yɛn amande fa krahye ne ho"

#: report.submitting
msgid "Sending report…"
msgstr "Repɔɔt no rekɔ…"

#: report.submitted
msgid "Crash report sent."
msgstr "Krahye repɔɔt no kɔ."

#: report.disabled
msgid "Crash reporting disabled."
msgstr "Edum krahye amandebɔ."

#: report.failed
msgid "Submission failed."
msgstr "Sabmihyɛn annkɔ yie."

#: report.unavailable
msgid "No report available."
msgstr "Repɔɔt biara nni nhɔ."
