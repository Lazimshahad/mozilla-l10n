#: rememberValue
msgid "Use Password Manager to remember this value."
msgstr "Fa Password Manager kae aba yi."

#: rememberPassword
msgid "Use Password Manager to remember this password."
msgstr "Fa Password Manager kae ehintasɛmfua yi."

#: savePasswordTitle
msgid "Confirm"
msgstr "Si-pi"

#. # 1st string is product name, 2nd is the username for the login, 3rd is the
#. #   login's hostname. Note that long usernames may be truncated.
#: saveLoginText
msgid "Do you want %1$S to remember the password for \"%2$S\" on %3$S?"
msgstr "Wo pɛ sɛ %1$S kae \"%2$S\" ne ehintasɛmfua wɔ %3$S?"

#. # 1st string is product name, 2nd is the login's hostname
#: saveLoginTextNoUsername
msgid "Do you want %1$S to remember this password on %2$S?"
msgstr "Wo pɛ sɛ %1$S kae ehintasɛmfua yi wɔ %2$S?"

#: notNowButtonText
msgid "&Not Now"
msgstr "&Nnyɛ Seiseiara"

#: notifyBarNotNowButtonText
msgid "Not Now"
msgstr "Nnyɛ Seiseiara"

#: notifyBarNotNowButtonAccessKey
msgid "N"
msgstr "N"

#: neverForSiteButtonText
msgid "Ne&ver for This Site"
msgstr "Ne&ver for This Site"

#: notifyBarNeverForSiteButtonText
msgid "Never for This Site"
msgstr "Mma Mmfa Nnsie Mma Saet Yi Ara Da"

#: notifyBarNeverForSiteButtonAccessKey
msgid "e"
msgstr "e"

#: rememberButtonText
msgid "&Remember"
msgstr "&Kae"

#: notifyBarRememberButtonText
msgid "Remember"
msgstr "Kae"

#: notifyBarRememberButtonAccessKey
msgid "R"
msgstr "K"

#: passwordChangeTitle
msgid "Confirm Password Change"
msgstr "Si Ehintasɛmfua Nsesae Pi"

#: passwordChangeText
msgid "Would you like to change the stored password for %S?"
msgstr "Wo bɛpɛ sɛ wo bɛsesa wo ehintasɛmfua ama %S?"

#: passwordChangeTextNoUser
msgid "Would you like to change the stored password for this login?"
msgstr "Wo bɛpɛ sɛ wo bɛsesa ehintasɛmfua a wɔde esie ama login yi?"

#: notifyBarChangeButtonText
msgid "Change"
msgstr "Sesa"

#: notifyBarChangeButtonAccessKey
msgid "C"
msgstr "C"

#: notifyBarDontChangeButtonText
msgid "Don't Change"
msgstr "Mma Nnsesa"

#: notifyBarDontChangeButtonAccessKey
msgid "D"
msgstr "D"

#: userSelectText
msgid "Please confirm which user you are changing the password for"
msgstr "Yɛsrɛ wo sɛ si dehye a wo resesa ehintasɛmfua ama no no pi"

#: hidePasswords
msgid "Hide Passwords"
msgstr "Sie Ehintasɛmfua"

#: hidePasswordsAccessKey
msgctxt "hidePasswordsAccessKey"
msgid "P"
msgstr "P"

#: showPasswords
msgid "Show Passwords"
msgstr "Kyerɛ Ehintasɛmfua"

#: showPasswordsAccessKey
msgctxt "showPasswordsAccessKey"
msgid "P"
msgstr "P"

#: noMasterPasswordPrompt
msgid "Are you sure you wish to show your passwords?"
msgstr "Wo gye di sɛ wo pɛ sɛ wo kyerɛ wo ehintasɛmfua?"

#: removeAllPasswordsPrompt
msgid "Are you sure you wish to remove all passwords?"
msgstr "Wo gye di sɛ wo pɛ sɛ wo yi wo ehintasɛmfua?"

#: removeAllPasswordsTitle
msgid "Remove all passwords"
msgstr "Yi ehintasɛmfua nyinara"

#: loginsSpielAll
msgid "Passwords for the following sites are stored on your computer:"
msgstr "Wɔde ehintasɛmfua a wɔwɔ hɔ ma nsaet yinom esie wo kɔmputa no so:"

#: loginsSpielFiltered
msgid "The following passwords match your search:"
msgstr "Ehintasɛmfua yinom de wo nhwehwɛe no di nhyia:"
