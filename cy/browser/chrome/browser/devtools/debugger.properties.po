#~ msgstr "Oedi"

#~ msgid "Resume"
#~ msgstr "Ailgychwyn"

#~ msgid "Paused"
#~ msgstr "Oedi"

#~ msgid "Running"
#~ msgstr "Rhedeg"

#~ msgid "Local"
#~ msgstr "Lleol"

