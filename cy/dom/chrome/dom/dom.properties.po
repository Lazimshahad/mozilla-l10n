msgstr ""
"Use of attributes' firstChild attribute is deprecated. Use value instead."

#: LastChildWarning
msgid ""
"Use of attributes' lastChild attribute is deprecated. Use value instead."
msgstr ""
"Use of attributes' lastChild attribute is deprecated. Use value instead."

#: PreviousSiblingWarning
msgid ""
"Use of attributes' previousSibling attribute is deprecated. It always "
"returns null."
msgstr ""
"Use of attributes' previousSibling attribute is deprecated. It always "
"returns null."

#: NextSiblingWarning
msgid ""
"Use of attributes' nextSibling attribute is deprecated. It always returns "
"null."
msgstr ""
"Use of attributes' nextSibling attribute is deprecated. It always returns "
"null."

#: AttributesWarning
msgid ""
"Use of attributes' attributes attribute is deprecated. It always returns "
"null."
msgstr ""
"Use of attributes' attributes attribute is deprecated. It always returns "
"null."

#: InsertBeforeWarning
msgid "Use of attributes' insertBefore() is deprecated. Use value instead."
msgstr "Use of attributes' insertBefore() is deprecated. Use value instead."

#: ReplaceChildWarning
msgid "Use of attributes' replaceChild() is deprecated. Use value instead."
msgstr "Use of attributes' replaceChild() is deprecated. Use value instead."

#: RemoveChildWarning
msgid "Use of attributes' removeChild() is deprecated. Use value instead."
msgstr "Use of attributes' removeChild() is deprecated. Use value instead."

#: AppendChildWarning
msgid "Use of attributes' appendChild() is deprecated. Use value instead."
msgstr "Use of attributes' appendChild() is deprecated. Use value instead."

#: CloneNodeWarning
msgid "Use of attributes' cloneNode() is deprecated."
msgstr "Use of attributes' cloneNode() is deprecated."

#: OwnerDocumentWarning
msgid "Use of attributes' ownerDocument attribute is deprecated."
msgstr "Use of attributes' ownerDocument attribute is deprecated."

#: NormalizeWarning
msgid "Use of attributes' normalize() is deprecated."
msgstr "Use of attributes' normalize() is deprecated."

#: IsSupportedWarning
msgid "Use of attributes' isSupported() is deprecated."
msgstr "Use of attributes' isSupported() is deprecated."

#: IsEqualNodeWarning
msgid "Use of attributes' isEqualNode() is deprecated."
msgstr "Use of attributes' isEqualNode() is deprecated."

#: TextContentWarning
msgid ""
"Use of attributes' textContent attribute is deprecated. Use value instead."
msgstr ""
"Use of attributes' textContent attribute is deprecated. Use value instead."

#: EnablePrivilegeWarning
msgid ""
"Use of enablePrivilege is deprecated.  Please use code that runs with the "
"system principal (e.g. an extension) instead."
msgstr ""
"Use of enablePrivilege is deprecated.  Please use code that runs with the "
"system principal (e.g. an extension) instead."

#: PositionWarning
msgid ""
"Use of XMLHttpRequest's progress events' position attribute is deprecated."
msgstr ""
"Mae'r defnydd o briodweddau safle digwyddiadau cynnydd XMLHttpRequest's yn "
"anghymeradwy."

#: TotalSizeWarning
msgid ""
"Use of XMLHttpRequest's progress events' totalSize attribute is deprecated."
msgstr ""
"Mae;r defnydd o briodwedd totalSize digwyddiad cynnydd XMLHttpRequest yn "
"anghymeradwy."

#: nsIJSONDecodeDeprecatedWarning
msgid "nsIJSON.decode is deprecated.  Please use JSON.parse instead."
msgstr "Mae nsIJSON.decode yn anghymeradwy. Defnyddiwch JSON.parse."

#: nsIJSONEncodeDeprecatedWarning
msgid "nsIJSON.encode is deprecated.  Please use JSON.stringify instead."
msgstr "Mae nsIJS.ON.encode anghymeradwy. Defnyddiwch JSON.stringify."

#: nsIDOMWindowInternalWarning
msgid "Use of nsIDOMWindowInternal is deprecated. Use nsIDOMWindow instead."
msgstr ""
"Mae'r defnyddd o nsIDOMWindowInternal yn anghymeradwy. Defnyddiwch "
"nsIDOMWindow ."

#: InputEncodingWarning
msgid "Use of inputEncoding is deprecated."
msgstr "Mae'r defnydd o inputEncoding yn anghymeradwy."

#. # LOCALIZATION NOTE: Do not translate "MozBeforePaint" and "mozRequestAnimationFrame"
#: MozBeforePaintWarning
msgid ""
"MozBeforePaint events are no longer supported.  mozRequestAnimationFrame "
"must be passed a non-null callback argument."
msgstr ""
"Nid yw digwyddiadau MozBeforePaint yn cael eu cynnal. Rhaid i "
"mozRequestAnimationFrame basio ymresymiad galw nôl an nwl."

#: FullScreenDeniedDisabled
msgid ""
"Request for full-screen was denied because full-screen API is disabled by "
"user preference."
msgstr ""
"Gwrthodwyd cais am sgrin lawn oherwydd mae'r API sgrin llawn wedi ei anablu "
"yn y dewisiadau personol."

#: FullScreenDeniedFocusedPlugin
msgid ""
"Request for full-screen was denied because a windowed plugin is focused."
msgstr ""
"Gwrthodwyd cais am sgrin lawn gan fod yr ategyn ffenestr wedi ei ffocysu."

#: FullScreenDeniedHidden
msgid ""
"Request for full-screen was denied because the document is no longer visible."
msgstr "Gwrthodwyd y cais am y sgrin lawn am nad yw'r ddogfen yn weladwy."

#: FullScreenDeniedIframeDisallowed
msgid ""
"Request for full-screen was denied because at least one of the document's "
"containing iframes does not have a \"mozallowfullscreen\" attribute."
msgstr ""
"Gwrthodwyd cais am sgrin lawn gan nad ose gan o leiaf un o ddogfennau yn "
"cynnwys iframes briodweddau \"mozallowfullscreen\"."

#: FullScreenDeniedNotInputDriven
msgid ""
"Request for full-screen was denied because Element.mozRequestFullScreen() "
"was not called from inside a short running user-generated event handler."
msgstr ""
"Gwrthodwyd cais am sgrin lawn gan nad oedd Element.mozRequestFullScreen()  "
"wedi ei alw o fewn triniwr digwyddiad rhedeg yn fyr gynhyrchwyn gan y "
"defnyddiwr."

#: FullScreenDeniedNotInDocument
msgid ""
"Request for full-screen was denied because requesting element is no longer "
"in its document."
msgstr ""
"Gwrthodwyd cais am sgrin lawn gan nad yw'r elfen oedd yn gofyn amdano "
"bellach o fewn ei ddogfen."

#: FullScreenDeniedMovedDocument
msgid ""
"Request for full-screen was denied because requesting element has moved "
"document."
msgstr ""
"Gwrthodwyd y cais am sgrin lawn oherwydd roedd mae'r elfen oedd yn gofyn "
"wedi newid dogfen."

#: FullScreenDeniedLostWindow
msgid "Request for full-screen was denied because we no longer have a window."
msgstr "Gwrthodwyd y cais am sgrin lawn oherwydd nad oes gennym ffenestr."

#: FullScreenDeniedSubDocFullScreen
msgid ""
"Request for full-screen was denied because a subdocument of the document "
"requesting full-screen is already full-screen."
msgstr ""
"Gwrthodwyd y cais am sgrin lawn oherwydd mae'r isddogfen y ddogfen sy'n "
"gofyn am sgrin lawn eisoes yn sgrin lawn."

#: FullScreenDeniedNotDescendant
msgid ""
"Request for full-screen was denied because requesting element is not a "
"descendant of the current full-screen element."
msgstr ""
"Gwrthodwyd cais am sgrin lawn oherwydd nad yw'r elfen sy'n gofyn yn "
"ddisgynnydd o'r elfen sgrin lawn gyfredol."

#: FullScreenDeniedNotFocusedTab
msgid ""
"Request for full-screen was denied because requesting element is not in the "
"currently focused tab."
msgstr ""
"Gwrthodwyd cais am sgrin lawn oherwydd nad oedd yr elfen o fewn y tab "
"canolbwyntio cyfredol."

#: RemovedFullScreenElement
msgid ""
"Exited full-screen because full-screen element was removed from document."
msgstr ""
"Gadawyd y sgrin llawn gan i'r elfen sgrin llawn gael ei dynnu o'r ddogfen."

#: FocusedWindowedPluginWhileFullScreen
msgid "Exited full-screen because windowed plugin was focused."
msgstr ""
"Gadawyd y sgrin llawn gan fod yr ategyn ffenestrog wedi ei ganolbwyntio."

#: HTMLMultipartXHRWarning
msgid ""
"HTML parsing in XMLHttpRequest is not supported for multipart responses."
msgstr ""
"Nid yw didoli HTML yn XMLHttpRequest yn cael ei gynnal ar gyfer ymatebion "
"amlddarn."

#: HTMLSyncXHRWarning
msgid ""
"HTML parsing in XMLHttpRequest is not supported in the synchronous mode."
msgstr ""
"Nid yw didoli HTML yn XMLHttpRequest yn cael ei gynnal yn y modd cydamserol."

#: InvalidRedirectChannelWarning
msgid ""
"Unable to redirect to %S because the channel doesn't implement "
"nsIWritablePropertyBag2."
msgstr ""
"Methu ailgyfeirio i %S oherwydd nad oedd y sianel yn gallu gweithredu "
"nsIWritablePropertyBag2."

#: ResponseTypeSyncXHRWarning
msgid ""
"Use of XMLHttpRequest's responseType attribute is no longer supported in the "
"synchronous mode in window context."
msgstr ""
"Nid yw defnydd o briodwedd responseType XMLHttpRequestyn cael ei gynal "
"bellach yn y modd cydamserol o fewn cyd-destyn ffenestr."

#: WithCredentialsSyncXHRWarning
msgid ""
"Use of XMLHttpRequest's withCredentials attribute is no longer supported in "
"the synchronous mode in window context."
msgstr ""
"Nid yw defnydd o briodwedd withCredentials XMLHttpRequestyn cael ei gynal "
"bellach yn y modd cydamserol o fewn cyd-destyn ffenestr."

#: TimeoutSyncXHRWarning
msgid ""
"Use of XMLHttpRequest's timeout attribute is not supported in the "
"synchronous mode in window context."
msgstr ""
"Nid yw'r defnydd o briodwedd allan o amser XMLHttpRequest yn cael ei gynnal "
"yn y modd cydweddu o fewn cyd-destun ffenestr."

#: JSONCharsetWarning
msgid ""
"An attempt was made to declare a non-UTF-8 encoding for JSON retrieved using "
"XMLHttpRequest. Only UTF-8 is supported for decoding JSON."
msgstr ""
"Gwnaed cais i ddatgan amgodiad an UTF-8 ar gyfer JSON wedi ei adfer gan "
"ddefnyddio XMLHttpRequest. Dim ond UTF-8 sy'n cael ei gynnal i dadgodio JSON."

#: MediaLoadExhaustedCandidates
msgid "All candidate resources failed to load. Media load paused."
msgstr "Methodd llwytho pob adnodd ymgeisydd. Oedwyd llwytho cyfrwng."

#: MediaLoadSourceMissingSrc
msgid "<source> element has no \"src\" attribute. Media resource load failed."
msgstr ""
"Nid oes gan elfen <source> briodwedd \"src\". Methodd llwytho'r adnoddau "
"cyfrwng."

#. # LOCALIZATION NOTE: %1$S is the Http error code the server returned (e.g. 404, 500, etc), %2$S is the URL of the media resource which failed to load.
#: MediaLoadHttpError
msgid "HTTP load failed with status %1$S. Load of media resource %2$S failed."
msgstr "Methodd llwytho HTTP gyda statws %1$S. Methodd llwytho adnoddau %2$S."

#. # LOCALIZATION NOTE: %S is the URL of the media resource which failed to load.
#: MediaLoadInvalidURI
msgid "Invalid URI. Load of media resource %S failed."
msgstr "URI annilys. Methodd llwytho adnoddau %S."

#. # LOCALIZATION NOTE: %1$S is the media resource's format/codec type (basically equivalent to the file type, e.g. MP4,AVI,WMV,MOV etc), %2$S is the URL of the media resource which failed to load.
#: MediaLoadUnsupportedType
msgid ""
"Specified \"type\" of \"%1$S\" is not supported. Load of media resource %2$S "
"failed."
msgstr ""
"Nid yw \"type\" o \"%1$S\" penodol yn cael eu cynal. Methodd llwytho "
"adnoddau %2$S."

#. # LOCALIZATION NOTE: %1$S is the MIME type HTTP header being sent by the web server, %2$S is the URL of the media resource which failed to load.
#: MediaLoadUnsupportedMimeType
msgid ""
"HTTP \"Content-Type\" of \"%1$S\" is not supported. Load of media resource "
"%2$S failed."
msgstr ""
"Nid yw \"Content-Type\" o \"%1$S\" HTTP yn cael eu cynnal. Methodd llwytho "
"adnoddau %2$S."

#. # LOCALIZATION NOTE: %S is the URL of the media resource which failed to load because of error in decoding.
#: MediaLoadDecodeError
msgid "Media resource %S could not be decoded."
msgstr "Nid oedd modd dadgodio adnodd cyfrwng %S."
