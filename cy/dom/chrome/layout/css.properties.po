#: MimeNotCss
msgid ""
"The stylesheet %1$S was not loaded because its MIME type, \"%2$S\", is not "
"\"text/css\"."
msgstr ""
"Cafodd dalen steil %1$S ddim ei llwytho am nad oedd ei fath MIME, \"%2$S\", "
"yn \"text/css\"."

#: MimeNotCssWarn
msgid ""
"The stylesheet %1$S was loaded as CSS even though its MIME type, \"%2$S\", "
"is not \"text/css\"."
msgstr ""
"Cafodd y ddalen steil %1$S ei llwytho fel CSS er nad yw ei fath MIME type, "
"\"%2$S\", yn \"text/css\"."

#: PEUnexpEOF2
msgid "Unexpected end of file while searching for %1$S."
msgstr "Diwedd ffeil annisgwyl wrth chwilio am '%1$S."

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr "Llinyn gofod gwyn yn unig i'w ddidoli fel rheol."

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "Gollwng datganiad."

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "Neidio i'r datganiad nesaf."

#: PEUnknownProperty
msgid "Unknown property '%1$S'."
msgstr "Priodwedd anhysbys '%1$S'."

#: PEValueParsingError
msgid "Error in parsing value for '%1$S'."
msgstr "Gwall didoli gwerth priodwedd '%1$S'."

#: PEExpectEndValue
msgid "Expected end of value but found '%1$S'."
msgstr "Disgwyl diwedd gwerth ond canfod '%1$S'."

#: PESkipAtRuleEOF
msgid "end of unknown at-rule"
msgstr "diwedd at-rule anhysbys"

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule '%1$S'."
msgstr "At-rule anhysbys neu wall didoli at-rule '%1$S'."

#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "llinyn set nod @charset rule"

#: PECharsetRuleNotString
msgid "Expected charset string but found '%1$S'."
msgstr "Disgwyl llinyn set nod ond canfod '%1$S'."

#: PEGatherMediaEOF
msgid "end of media list in @import or @media rule"
msgstr "diwedd rheol rhestr cyfrwng yn @import neu @media"

#: PEGatherMediaNotComma
msgid "Expected ',' in media list but found '%1$S'."
msgstr "Disgwyl ',' yn rhestr cyfrwng ond canfod '%1$S'."

#: PEGatherMediaNotIdent
msgid "Expected identifier in media list but found '%1$S'."
msgstr "Disgwyl dynodydd mewn rhestr cyfrwng ond canfod '%1$S'."

#: PEImportNotURI
msgid "Expected URI in @import rule but found '%1$S'."
msgstr "Disgwyl URI o fewn rheol @import ond canfod '%1$S'."

#: PEImportBadURI
msgid "Invalid URI in @import rule: '%1$S'."
msgstr "URI annilys yn @import rule: '%1$S'."

#: PEImportUnexpected
msgid "Found unexpected '%1$S' within @import."
msgstr "Canfod '%1$S' annisgwyl o fewn @import."

#: PEGroupRuleEOF
msgid "end of @media or @-moz-document rule"
msgstr "diwedd rheol @media neu @-moz-document"

#: PEGroupRuleNestedAtRule
msgid "%1$S rule not allowed within @media or @-moz-document rule."
msgstr ""
"Rheol %1$S ddim yn cael ei ganiatau o fewn rheol @media na rheol @-moz-"
"document."

#: PEMozDocRuleBadFunc
msgid ""
"Expected url(), url-prefix(), or domain() in @-moz-document rule but found "
"'%1$S'."
msgstr ""
"Disgwyl url(), rhag-ur(), neu barth() o fewn rheol @-moz-document ond canfod "
"'%1$S'."

#: PEMozDocRuleNotURI
msgid "Expected URI in @-moz-document rule but found '%1$S'."
msgstr "Disgwyl URI o fewn rheol @moz-document ond canfod '%1$S'."

#: PEMozDocRuleNotString
msgid ""
"Expected string in @-moz-document rule regexp() function but found '%1$S'."
msgstr ""
"Disgwyl llinyn swyddogaeth rheol @-moz-document egexp() ond canfod '%1$S'."

#: PEMozDocRuleEOF
msgid "next URI in @-moz-document rule"
msgstr "URI yn y rheol @-moz-document nesaf"

#: PEAtNSPrefixEOF
msgid "namespace prefix in @namespace rule"
msgstr "rhagddodiad gofod enw o fewn @namespace"

#: PEAtNSURIEOF
msgid "namespace URI in @namespace rule"
msgstr "URL gofod o fewn rheol @namespace"

#: PEAtNSUnexpected
msgid "Unexpected token within @namespace: '%1$S'."
msgstr "Tocyn annisgwyl o fewn @namespace: '%1$S'."

#: PEKeyframeNameEOF
msgid "name of @keyframes rule."
msgstr "enw rheol @keyframes."

#: PEKeyframeBadName
msgid "Expected identifier for name of @keyframes rule."
msgstr "Enw disgwyliedig rheol @keyframes."

#: PEKeyframeBrace
msgid "Expected opening { of @keyframes rule."
msgstr "Agoriad disgwyiedig rheol { @keyframes."

#: PESkipDeclBraceEOF
msgid "closing } of declaration block"
msgstr "cau } rhwystr datganiad"

#: PESkipRSBraceEOF
msgid "closing } of invalid rule set"
msgstr "cau } set rheol annilys"

#: PEBadSelectorRSIgnored
msgid "Ruleset ignored due to bad selector."
msgstr "Anwybyddwyd set rheolau oherwydd dewisydd gwallus."

#: PEBadSelectorKeyframeRuleIgnored
msgid "Keyframe rule ignored due to bad selector."
msgstr "Rheol keyframe wedi ei anwybyddu oherwydd dewisydd gwael."

#: PESelectorListExtraEOF
msgid "',' or '{'"
msgstr ",' neu '{'"

#: PESelectorListExtra
msgid "Expected ',' or '{' but found '%1$S'."
msgstr "Disgwyl ',' neu '{' ond canfod '%1$S'."

#: PESelectorGroupNoSelector
msgid "Selector expected."
msgstr "Disgwyl dewisydd"

#: PESelectorGroupExtraCombinator
msgid "Dangling combinator."
msgstr "Cyfuniadur rhydd"

#: PEClassSelEOF
msgid "class name"
msgstr "enw dosbarth"

#: PEClassSelNotIdent
msgid "Expected identifier for class selector but found '%1$S'."
msgstr "Disgwyl dynodydd dewisydd dosbarth ond canfod '%1$S'."

#: PETypeSelEOF
msgid "element type"
msgstr "math o elfen"

#: PETypeSelNotType
msgid "Expected element name or '*' but found '%1$S'."
msgstr "Disgwyl enw elfen neu '*' ond canfod '%1$S'."

#: PEUnknownNamespacePrefix
msgid "Unknown namespace prefix '%1$S'."
msgstr "Rhagddodiad bwlch enw anhysbys '%1$S'."

#: PEAttributeNameEOF
msgid "attribute name"
msgstr "enw priodoledd"

#: PEAttributeNameExpected
msgid "Expected identifier for attribute name but found '%1$S'."
msgstr "Disgwyl dynodydd enw priodoledd ond canfod '%1$S'."

#: PEAttributeNameOrNamespaceExpected
msgid "Expected attribute name or namespace but found '%1$S'."
msgstr "Disgwyl enw priodoledd neu gyswllt ond canfod '%1$S'."

#: PEAttSelNoBar
msgid "Expected '|' but found '%1$S'."
msgstr "Disgwyl '|' ond canfod '%1$S'."

#: PEAttSelInnerEOF
msgid "part of attribute selector"
msgstr "rhan o'r dewisydd priodoledd"

#: PEAttSelUnexpected
msgid "Unexpected token in attribute selector: '%1$S'."
msgstr "Dewisydd priodoledd mewn tocyn annisgwyl: '%1$S'."

#: PEAttSelValueEOF
msgid "attribute value"
msgstr "gwerth priodwedd"

#: PEAttSelCloseEOF
msgid "']' to end attribute selector"
msgstr "]' i ddiwedd dewisydd priodwedd"

#: PEAttSelNoClose
msgid "Expected ']' to terminate attribute selector but found '%1$S'."
msgstr "Disgwyl ']' i atal dewisydd priodwedd ond canfod '%1$S'."

#: PEAttSelBadValue
msgid ""
"Expected identifier or string for value in attribute selector but found "
"'%1$S'."
msgstr ""
"Disgwyl dynodydd neu linyn gwerth mewn dewisydd priodoledd ond canfod '%1$S'."

#: PEPseudoSelEOF
msgid "name of pseudo-class or pseudo-element"
msgstr "enw ffug ddosbarth neu ffug elfen"

#: PEPseudoSelBadName
msgid ""
"Expected identifier for pseudo-class or pseudo-element but found '%1$S'."
msgstr "Disgwyl dynodydd ffug ddosbarth neu ffug elfen ond canfod '%1$S'."

#: PEPseudoSelNonFunc
msgid ""
"Function token for non-function pseudo-class or pseudo-element, or the other "
"way around, when reading '%1$S'."
msgstr ""
"Tocyn swyddogaeth ar gyfer ffug ddosbarth neu ffug elfen answyddogaethol, "
"neu'n groes, wrth ddarllen '%1$S'."

#: PEPseudoSelNotPE
msgid "Expected pseudo-element but found '%1$S'."
msgstr "Disgwyl ffug elfen ond canfod '%1$S'."

#: PEPseudoSelDoubleNot
msgid "Negation pseudo-class can't be negated '%1$S'."
msgstr "Methu negyddu dosbarth ffug negyddiad '%1$S'."

#: PEPseudoSelPEInNot
msgid "Pseudo-elements can't be negated '%1$S'."
msgstr "Methu negyddu ffug elfennau '%1$S'."

#: PEPseudoSelNewStyleOnly
msgid "This pseudo-element must use the \"::\" form: '%1$S'."
msgstr "Rhaid i'r ffug elfen ddefnyddio ffurf \"::\" : '%1$S'."

#: PEPseudoSelTrailing
msgid ""
"Found trailing token after pseudo-element, which must be the last part of a "
"selector:  '%1$S'."
msgstr ""
"Canfod gair dilynol wedi ffug elfen, ddylai fod yn rhan olaf dewisydd:  "
"'%1$S'."

#: PEPseudoSelMultiplePE
msgid "Extra pseudo-element '%1$S'."
msgstr "Ffug elfen ychwanegol '%1$S'."

#: PEPseudoSelUnknown
msgid "Unknown pseudo-class or pseudo-element '%1$S'."
msgstr "Ffug ddosbarth neu ffug elfen '%1$S'."

#: PENegationEOF
msgid "selector within negation"
msgstr "dewisydd o fewn negydiad"

#: PENegationBadInner
msgid "Malformed simple selector as negation pseudo-class argument '%1$S'."
msgstr "dewisydd syml anffurf fel negydydd ymresymiad ffug ddosbarth '%1$S'."

#: PENegationNoClose
msgid "Missing closing ')' in negation pseudo-class '%1$S'."
msgstr "Cau ')' coll mewn ffug ddosbarth negyddiad '%1$S'."

#: PENegationBadArg
msgid "Missing argument in negation pseudo-class '%1$S'."
msgstr "Ymresymiad coll mewn ffug ddosbarth negydydd '%1$S'."

#: PEPseudoClassArgEOF
msgid "argument to pseudo-class selector"
msgstr "ymresymiad ar gyfer dewisydd ffug-ddosbarth"

#: PEPseudoClassArgNotIdent
msgid "Expected identifier for pseudo-class parameter but found '%1$S'."
msgstr "Disgwyl dynodwr paramedr ffug-ddosbarth ond wedi canfod '%1$S'."

#: PEPseudoClassArgNotNth
msgid "Expected part of argument to pseudo-class but found '%1$S'."
msgstr "Disgwyl rhan o ymresymiad ffug ddosbarth ond cafwyd  '%1$S'."

#: PEPseudoClassNoClose
msgid "Missing closing ')' in pseudo-class, found '%1$S' instead."
msgstr "')' cau ar goll yn y ffug-ddosbarth, canfod  '%1$S' yn lle hynny."

#: PEPseudoClassNoArg
msgid "Missing argument in pseudo-class '%1$S'."
msgstr "Ymresymiad ar goll yn ffug-ddosbarth '%1$S'."

#: PESelectorEOF
msgid "selector"
msgstr "dewisydd"

#: PEBadDeclBlockStart
msgid "Expected '{' to begin declaration block but found '%1$S'."
msgstr "Disgwyl '{' i gychwyn bloc datgan ond canfod '%1$S'."

#: PEColorEOF
msgid "color"
msgstr "lliw"

#: PEColorNotColor
msgid "Expected color but found '%1$S'."
msgstr "Disgwyl lliw ond canfod '%1$S'."

#: PEColorComponentEOF
msgid "color component"
msgstr "cydran lliw"

#: PEExpectedPercent
msgid "Expected a percentage but found '%1$S'."
msgstr "Disgwyl canran ond canfod  '%1$S'."

#: PEExpectedInt
msgid "Expected an integer but found '%1$S'."
msgstr "Disgwyl cyfanrif ond canfod '%1$S'."

#: PEColorBadRGBContents
msgid "Expected number or percentage in rgb() but found '%1$S'."
msgstr "Disgwyl rhif neu ganran mewn rgb() ond canfod '%1$S'."

#: PEColorComponentBadTerm
msgid "Expected '%2$S' but found '%1$S'."
msgstr "Disgwyl '%2$S' ond canfod '%1$S'."

#: PEColorHueEOF
msgid "hue"
msgstr "arlliw"

#: PEExpectedComma
msgid "Expected ',' but found '%1$S'."
msgstr "Disgwyl ',' ond canfod '%1$S'."

#: PEColorSaturationEOF
msgid "saturation"
msgstr "dirlenwad"

#: PEColorLightnessEOF
msgid "lightness"
msgstr "ysgafnder"

#: PEColorOpacityEOF
msgid "opacity in color value"
msgstr "didreiddedd mewn gwerth lliw"

#: PEExpectedNumber
msgid "Expected a number but found '%1$S'."
msgstr "Disgwyl rhif ond canfod '%1$S'."

#: PEExpectedCloseParen
msgid "Expected ')' but found '%1$S'."
msgstr "Disgwyl ')' ond canfod '%1$S'."

#: PEDeclEndEOF
msgid "';' or '}' to end declaration"
msgstr ";' neu '}' i gau'r datganiad"

#: PEParseDeclarationNoColon
msgid "Expected ':' but found '%1$S'."
msgstr "Disgwyl ':' ond canfod '%1$S'."

#: PEParseDeclarationDeclExpected
msgid "Expected declaration but found '%1$S'."
msgstr "Disgwyl cyhoeddiad ond canfod '%1$S'."

#: PEEndOfDeclEOF
msgid "end of declaration"
msgstr "diwedd datganiad"

#: PEImportantEOF
msgid "important"
msgstr "pwysig"

#: PEExpectedImportant
msgid "Expected 'important' but found '%1$S'."
msgstr "Disgwyl pwysig' ond canfod '%1$S'."

#: PEBadDeclEnd
msgid "Expected ';' to terminate declaration but found '%1$S'."
msgstr "Disgwyliwyd ';' i derfynu datganiad ond canfod '%1$S'."

#: PEBadDeclOrRuleEnd2
msgid "Expected ';' or '}' to terminate declaration but found '%1$S'."
msgstr "Disgwyl  ';' neu '}' i derfynu datganiad ond canfod '%1$S'."

#: PEInaccessibleProperty2
msgid "Cannot specify value for internal property."
msgstr "Methu pennu gwerth ar gyfer priodwedd mewnol."

#: PECommentEOF
msgid "end of comment"
msgstr "diwedd sylw"

#: SEUnterminatedString
msgid "Found unclosed string '%1$S'."
msgstr "Canfuwyd llinyn heb ei gloi '%1$S'."

#: PEFontDescExpected
msgid "Expected font descriptor but found '%1$S'."
msgstr "Disgwyl disgrifiad ffont ond wedi canfod  '%1$S'."

#: PEUnknownFontDesc
msgid "Unknown descriptor '%1$S' in @font-face rule."
msgstr "Disgrifiad anhysbys '%1$S' yn @font-face rule."

#: PEMQExpectedExpressionStart
msgid "Expected '(' to start media query expression but found '%1$S'."
msgstr ""
"Disgwyl '(' i gychwyn mynegiad ymholiad cyfrwng ond wedi canfod '%1$S'."

#: PEMQExpressionEOF
msgid "contents of media query expression"
msgstr "cynnwys mynegiad ymholiad cyfrwng"

#: PEMQExpectedFeatureName
msgid "Expected media feature name but found '%1$S'."
msgstr "Disgwyl enw nodwedd cyfrwng ond wedi canfod  '%1$S'."

#: PEMQExpectedFeatureNameEnd
msgid "Expected ':' or ')' after media feature name but found '%1$S'."
msgstr ""
"Disgwyl  ':' neu ')' ar ddiwedd enw nodwedd cyfrwng ond wedi canfod '%1$S'."

#: PEMQNoMinMaxWithoutValue
msgid "Media features with min- or max- must have a value."
msgstr "Rhaid i nodweddion cyfrwng sydd â min- neu max- fod â gwerth."

#: PEMQExpectedFeatureValue
msgid "Found invalid value for media feature."
msgstr "Canfod gwerth annilys ar gyfer nodwedd cyfrwng."

#: PEBadFontBlockStart
msgid "Expected '{' to begin @font-face rule but found '%1$S'."
msgstr "Disgwyl '(' i ddechrau @font-face ond canfod '%1$S'."

#: PEBadFontBlockEnd
msgid "Expected '}' to end @font-face rule but found '%1$S'."
msgstr "Disgwyl ')' i ddiweddu @ font-face, ond canfod '%1$S'."

#: PEAnonBoxNotAlone
msgid "Did not expect anonymous box."
msgstr "Heb ddisgwyl blwch mewnforio."
