#: warn.description
msgid ""
"&brandShortName; can alert you to the security status of the web page you "
"are viewing. Set &brandShortName; to show a warning and ask permission "
"before:"
msgstr ""
"&brandShortName; can alert you to the security status of the web page you "
"are viewing. Set &brandShortName; to show a warning and ask permission "
"before:"

#: warn.enteringsecure
msgid "Loading a page that supports encryption"
msgstr "Loading a page that supports encryption"

#: warn.enteringsecure.accesskey
msgid "L"
msgstr "L"

#: warn.enteringweak
msgid "Loading a page that uses low-grade encryption"
msgstr "Loading a page that uses low-grade encryption"

#: warn.enteringweak.accesskey
msgid "o"
msgstr "o"

#: warn.insecurepost
msgid "Sending form data from an unencrypted page to an unencrypted page"
msgstr "Sending form data from an unencrypted page to an unencrypted page"

#: warn.insecurepost.accesskey
msgid "S"
msgstr "S"

#: warn.leavingsecure
msgid "Leaving a page that supports encryption"
msgstr "Leaving a page that supports encryption"

#: warn.leavingsecure.accesskey
msgid "a"
msgstr "a"

#: warn.secureredirect
msgid "Redirecting from one encrypted page to another"
msgstr "Redirecting from one encrypted page to another"

#: warn.secureredirecttoinsecure
msgid "Redirecting from an encrypted page to an unencrypted page"
msgstr "Redirecting from an encrypted page to an unencrypted page"

#: warn.viewmixed
msgid "Viewing a page with an encrypted/unencrypted mix"
msgstr "Viewing a page with an encrypted/unencrypted mix"

#: warn.viewmixed.accesskey
msgid "V"
msgstr "V"

