#: validation.crlmanager.label
msgid "Manage CRLs"
msgstr "Stiùirich na LTC"

#: validation.crlmanager.description
msgid ""
"These Certificate Revocation Lists (CRL) are stored in your certificate "
"database:"
msgstr ""
"Tha liostaichean nan teisteanasan a chaidh a chùl-ghairm (LTC) seo 'gan "
"stòradh ann an stòr-dàta nan teisteanasan agad:"

#: validation.crlname.label
msgid "Name"
msgstr "Ainm"

#: validation.crllastupdate.label
msgid "Last Update"
msgstr "An t-ùrachadh mu dheireadh"

#: validation.crlnextupdate.label
msgid "Next Update"
msgstr "An t-ath-ùrachadh"

#: validation.crlautoupdateenabled.label
msgid "Auto Update"
msgstr "Ùrachadh leis fhèin"

#: validation.crlautoupdatestatus.label
msgid "Auto Update Status"
msgstr "Inbhe an ùrachaidh leis fhèin"

#: validation.deletecrl.label
#: validation.deletecrl.accesskey
msgid "&Delete"
msgstr "&Sguab às"

#: validation.updatecrl.label
#: validation.updatecrl.accesskey
msgid "&Update"
msgstr "Ù&raich"

#: validation.advanced.label
#: validation.advanced.accesskey
msgid "&Settings"
msgstr "&Roghainnean"

#: validation.crl.autoupdate.title
msgid "Automatic CRL Update Preferences"
msgstr "Roghainnean airson ùrachadh an LTC leis fhèin"

#: validation.crl.autoupdate.enable.label
msgid "Enable Automatic Update for this CRL"
msgstr "Cuir ùrachadh leis fhèin an comas airson an LTC seo"

#: validation.crl.autoupdate.time.label1
msgid "Update"
msgstr "Ùraich"

#: validation.crl.autoupdate.time.label2
msgid "Day(s) before Next Update date"
msgstr "Là(ithean) gu ceann-là an ath-ùrachaidh"

#: validation.crl.autoupdate.freq.label1
msgid "Update every"
msgstr "Ùraich a h-uile"

#: validation.crl.autoupdate.freq.label2
msgid "Day(s)"
msgstr "Là(ithean)"

#: validation.crl.autoupdate.url.label
msgid "CRL would be imported From:"
msgstr "Rachadh an LTC ion-phortadh o:"

#: crl.import.status.title
msgid "CRL Import Status"
msgstr "Inbhe ion-phortadh an LTC"

#: crl.import.success.message
msgid "The Certificate Revocation List (CRL) was successfully imported."
msgstr ""
"Chaidh liosta nan teisteanasan a chaidh a chù-ghairm (LTC) ion-phortadh gu "
"soirbheachail."

#: crl.issuer.label
msgid "CRL Issued By:"
msgstr "LTC air fhoillseachadh le:"

#: crl.issuer.org.label
msgid "Organization: "
msgstr "Buidheann: "

#: crl.issuer.orgunit.label
msgid "Unit: "
msgstr "Aonad: "

#: crl.import.nextupdate.label
msgid "Next Update On: "
msgstr "An t-ath-ùrachadh: "

#: crl.autoupdate.fail.cnt.label
msgid "Previous Consecutive Update Failures: "
msgstr "Dh'fhàillig ùrachaidhean an dèidh a chèile roimhe:"

#: crl.autoupdate.fail.reason.label
msgid "Details of Last Update Failure: "
msgstr "Mion-fhiosrachadh air fàillinn mu dheireadh an ùrachaidh:"

#: edit.button
msgid "Settings"
msgstr "Roghainnean"
