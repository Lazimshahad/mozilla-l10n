#. #--------------------------------------------------------------------
#. #   Install Actions
#. #--------------------------------------------------------------------
#: InstallFile
msgid "Installing: %s"
msgstr "A' stàladh: %s"

#: InstallSharedFile
msgid "Installing Shared File: %s"
msgstr "A' stàladh faidhle co-roinnte: %s"

#: ReplaceFile
msgid "Replacing: %s"
msgstr "A' cur an àite: %s"

#: ReplaceSharedFile
msgid "Replacing Shared File: %s"
msgstr "A' cur an àite an fhaidhle cho-roinnte: %s"

#: SkipFile
msgid "Skipping: %s"
msgstr "A' leumadh thairis air: %s"

#: SkipSharedFile
msgid "Skipping Shared File: %s"
msgstr "A' leumadh thairis air an fhaidhle cho-roinnte: %s"

#: DeleteFile
msgid "Deleting file: %s"
msgstr "A' sguabadh às an fhaidhle: %s"

#: DeleteComponent
msgid "Deleting component: %s"
msgstr "A' sguabadh às na co-phàirte: %s"

#: Execute
msgid "Executing: %s"
msgstr "A' gnìomhachadh: %s"

#: ExecuteWithArgs
msgid "Executing: %s with argument: %s"
msgstr "A' gnìomhachadh: %s leis an argamaid: %s"

#: CopyFile
msgid "Copy File: %s to %s"
msgstr "A' dèanamh lethbhreac de: %s gu %s"

#: ExecuteFile
msgid "Execute File: %s"
msgstr "Gnìomhaich am faidhle: %s"

#: ExecuteFileWithArgs
msgid "Execute File: %s with argument: %s"
msgstr "Gnìomhaich am faidhle: %s leis an argamaid: %s"

#: MoveFile
msgid "Move File: %s to %s"
msgstr "Gluais am faidhle: %s gu %s"

#: RenameFile
msgid "Rename File: %s to %s"
msgstr "Cuir ainm eile air an fhaidhle: %s gu %s"

#: CreateFolder
msgid "Create Folder: %s"
msgstr "Cruthaich pasgan: %s"

#: RemoveFolder
msgid "Remove Folder: %s"
msgstr "Thoir air falbh am pasgan: %s"

#: RenameFolder
msgid "Rename Folder: %s to %s"
msgstr "Cuir ainm eile air a' phasgan: %s gu %s"

#: WindowsShortcut
msgid "Windows Shortcut: %s"
msgstr "Ath-ghoirid Windows: %s"

#: MacAlias
msgid "Mac Alias: %s"
msgstr "Ainm-brèige Mhic an Tòisich: %s"

#: WindowsRegisterServer
msgid "Windows Register Server: %s"
msgstr "Frithealaiche Clàr Windows: %s"

#: UnknownFileOpCommand
msgid "Unknown file operation command!"
msgstr "Òrdugh obrachadh faidhle neo-aithnichte!"

#: Patch
msgid "Patching: %s"
msgstr "A' cur brèid: %s"

#: Uninstall
msgid "Uninstalling: %s"
msgstr "A' dì-stàladh: %s"

#: RegSkin
msgid "Register Skin: %s"
msgstr "Clàraich craiceann: %s"

#: RegLocale
msgid "Register Locale: %s"
msgstr "Clàraich dreach ionadail: %s"

#: RegContent
msgid "Register Content: %s"
msgstr "Clàraich susbaint: %s"

#: RegPackage
msgid "Register Package: %s"
msgstr "Clàraich pacaid: %s"

#. #--------------------------------------------------------------------#   Dialog Messages#--------------------------------------------------------------------
#: ApplyNowSkin
msgid "Use this theme"
msgstr "Cleachd an t-ùrlar seo"

#: ApplyNowLocale
msgid "Use this locale"
msgstr "Cleachd an dreach ionadail seo"

#: ConfirmSkin
msgid "Install the theme \"%1$S\" from %2$S?"
msgstr "A bheil thu airson an t-ùrlar \"%1$S\" o %2$S a stàladh?"

#: ConfirmLocale
msgid "Install the locale \"%1$S\" from %2$S?"
msgstr "A bheil thu airson an dreach ionadail \"%1$S\" o %2$S a stàladh?"

#: OK
msgid "Install"
msgstr "Stàlaich"

#: progress.queued
msgid "Queued"
msgstr "Sa chiutha"

#: progress.downloading
msgid "Downloading…"
msgstr "A' luchdadh a-nuas…"

#: progress.downloaded
msgid "Downloaded"
msgstr "Air a luchdadh a-nuas"

#: progress.installing
msgid "Installing…"
msgstr "'Ga stàladh…"

#: Unsigned
msgid "Unsigned"
msgstr "Gun ainm ris"

#. #--------------------------------------------------------------------
#. #   Miscellaneous
#. #--------------------------------------------------------------------
#: ERROR
msgid "ERROR"
msgstr "MEARACHD"

#: error0
msgid "Success"
msgstr "Shoirbhich leis"

#: error999
msgid "Restart to complete"
msgstr "Ag ath-thòiseachadh gus a thoirt gu buil"

#: error-202
msgid "Access denied"
msgstr "Inntrigeadh air a dhiùltadh"

#: error-203
msgid ""
"Unexpected installation error\n"
"Review the Error Console log for more details."
msgstr ""
"Mearachd stàlaidh ris nach robh dùil\n"
"Cuir sùil air loga consoil nam mearachdan airson barrachd fiosrachaidh."

#: error-204
msgid "Install script not found"
msgstr "Cha deach an sgriobt stàlaidh a lorg"

#: error-207
msgid "Not a valid install package"
msgstr "Chan e pacaid stàlaidh dhligheach a tha ann"

#: error-208
msgid "Invalid argument"
msgstr "Argamaid mhì-dhligheach"

#: error-210
msgid "User canceled"
msgstr "Air a sgur dheth le cleachdaiche"

#: error-214
msgid "Required file does not exist"
msgstr "Chan eil am faidhle air a bheil feum ann"

#: error-215
msgid "Read only"
msgstr "Ri leughadh a-mhàin"

#: error-218
msgid "AppleSingle extraction error"
msgstr "Mearachd às-tharraing AppleSingle"

#: error-219
msgid "Invalid path"
msgstr "Slighe mhì-dhligheach"

#: error-225
msgid "EXTRACTION_FAILED"
msgstr "EXTRACTION_FAILED"

#: error-227
msgid "Canceled"
msgstr "Air a sgur dheth"

#: error-228
msgid "Download error"
msgstr "Mearachd leis an luchdadh a-nuas"

#: error-229
msgid "Script error"
msgstr "Mearachd le sgriobt"

#: error-230
msgid "Already exists"
msgstr "Tha e ann mu thràth"

#: error-235
msgid "Out of space"
msgstr "Gun àite"

#: error-239
msgid "Chrome registration failed"
msgstr "Dh'fhàillig clàrachadh Chrome"

#: error-240
msgid "Unfinished install"
msgstr "Stàladh gun chrìochnachadh"

#: error-244
msgid "Unsupported package"
msgstr "Pacaid nach eil taic ann dha"

#: error-260
msgid "Signing could not be verified."
msgstr "Cha deach cur ris an ainm a dhearbhadh."

#: error-261
msgid "Invalid file hash (possible download corruption)"
msgstr ""
"Faidhle hais mì-dhligheach (truailleadh rè an luchdaidh a-nuas 's dòcha)"

#: error-262
msgid "Unknown or invalid file hash type"
msgstr "Seòrsa de dh'fhaidhle hais a tha neo-aithnichte no mì-dhligheach"

#: error-299
msgid "Out of memory"
msgstr "Dh'fhalbh a' chuimhne air"

#. # there are other error codes, either rare or obsolete,
#. # that are not worth translating at this time.
#: unknown.error
msgid "Unexpected error %S"
msgstr "Mearachd %S ris nach robh dùil"
