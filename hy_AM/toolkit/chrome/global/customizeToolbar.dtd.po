#: dialog.title
msgid "Customize Toolbar"
msgstr "Կարգավորել Մանրավահանակը"

#. Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger
#: dialog.style
msgid "width: 86ch; height: 36em;"
msgstr "width: 700px; height: 430px;"

#: instructions.description
msgid "You can add or remove items by dragging to or from the toolbars."
msgstr ""
"Դեպի մանրավահանակը կամ մանրավահանակից քաշելով կարող եք բաներ ավելացնել կամ "
"հեռացնել:"

#: show.label
msgid "Show:"
msgstr "Ցուցադրել."

#: iconsAndText.label
msgid "Icons and Text"
msgstr "Մանրանկարներ և Տեքստ"

#: icons.label
msgid "Icons"
msgstr "Մանրանկարներ"

#: text.label
msgid "Text"
msgstr "Տեքստ"

#: useSmallIcons.label
msgid "Use Small Icons"
msgstr "Փոքր Մանրանկարներ Գործածել"

#: restoreDefaultSet.label
msgid "Restore Default Set"
msgstr "Վերականգնել Նախնականը"

#: addNewToolbar.label
msgid "Add New Toolbar"
msgstr "Ավելացնել Նոր Վահանակ"

#: saveChanges.label
msgid "Done"
msgstr "Կատարված է"

#: undoChanges.label
msgid "Undo Changes"
msgstr "Փոփոխությունները Հետադարձել"
