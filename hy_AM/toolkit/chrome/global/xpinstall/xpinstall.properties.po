#. #--------------------------------------------------------------------
#. #   Install Actions
#. #--------------------------------------------------------------------
#: InstallFile
msgid "Installing: %s"
msgstr "Տեղադրում եմ՝. %s"

#: InstallSharedFile
msgid "Installing Shared File: %s"
msgstr "Տեղադրում եմ համընդհանուր օգտագործվող Ֆայլերը՝. %s"

#: ReplaceFile
msgid "Replacing: %s"
msgstr "Փոխարինել՝. %s"

#: ReplaceSharedFile
msgid "Replacing Shared File: %s"
msgstr "Փողարինում եմ համատեղ օգտագործվող ֆայլ՝. %s"

#: SkipFile
msgid "Skipping: %s"
msgstr "Անցնել՝ %s"

#: SkipSharedFile
msgid "Skipping Shared File: %s"
msgstr "Բաց եմ թողնում %s համատեգ օգտագործման ֆայլը"

#: DeleteFile
msgid "Deleting file: %s"
msgstr "Հեռացնում եմ ֆայլ՝. %s"

#: DeleteComponent
msgid "Deleting component: %s"
msgstr "Հեռացնում եմ բաղադրիչ՝. %s"

#: Execute
msgid "Executing: %s"
msgstr "Կատարում եմ՝. %s"

#: ExecuteWithArgs
msgid "Executing: %s with argument: %s"
msgstr "Կատարում եմ %s­ը %s փաստարկով"

#: CopyFile
msgid "Copy File: %s to %s"
msgstr "Ֆայլը կրկնորինակել՝. %s­ից %s"

#: ExecuteFile
msgid "Execute File: %s"
msgstr "Կատարել Ֆայլը՝. %s"

#: ExecuteFileWithArgs
msgid "Execute File: %s with argument: %s"
msgstr "Կատարել %s ֆայլը %s փաստարկով"

#: MoveFile
msgid "Move File: %s to %s"
msgstr "Ֆայլը տեղափոխել՝. %s zu %s"

#: RenameFile
msgid "Rename File: %s to %s"
msgstr "Ֆայլը վերանվանել՝. %s­ից %s"

#: CreateFolder
msgid "Create Folder: %s"
msgstr "Ստեղծել պանակ՝. %s"

#: RemoveFolder
msgid "Remove Folder: %s"
msgstr "Ջնջել պանակը՝. %s"

#: RenameFolder
msgid "Rename Folder: %s to %s"
msgstr "Պանակը վերանվանել: %s­ից %s"

#: WindowsShortcut
msgid "Windows Shortcut: %s"
msgstr "Windows-Համառոտում. %s"

#: MacAlias
msgid "Mac Alias: %s"
msgstr "Mac փոխաբերում՝. %s"

#: WindowsRegisterServer
msgid "Windows Register Server: %s"
msgstr "Windows Գրանցման Սպասարկիչ. %s"

#: UnknownFileOpCommand
msgid "Unknown file operation command!"
msgstr "Անհայտ ֆայլային գործառույթ:"

#: Patch
msgid "Patching: %s"
msgstr "Կարկատում եմ՝. %s"

#: Uninstall
msgid "Uninstalling: %s"
msgstr "Հեռացնում եմ՝ %s"

#: RegSkin
msgid "Register Skin: %s"
msgstr "Գրանցել Մորթին՝. %s"

#: RegLocale
msgid "Register Locale: %s"
msgstr "Լեզուն գրանցել՝. %s"

#: RegContent
msgid "Register Content: %s"
msgstr "Գրանցել Պարունակությունը ՝. %s"

#: RegPackage
msgid "Register Package: %s"
msgstr "Փաթեթը գրանցել՝. %s"

#. #--------------------------------------------------------------------
#. #   Dialog Messages
#. #--------------------------------------------------------------------
#: ApplyNowSkin
msgid "Use this theme"
msgstr "Օգտագործել այս ձևավորումը"

#: ApplyNowLocale
msgid "Use this locale"
msgstr "Կիրառել այս լեզուն"

#: ConfirmSkin
msgid "Install the theme \"%1$S\" from %2$S?"
msgstr "Ցանկանու՞մ եք \"%1$S\" ձեվավորումը %2$S­ից տեղադրել:"

#: ConfirmLocale
msgid "Install the locale \"%1$S\" from %2$S?"
msgstr "Ցանկանու՞մ եք \"%1$S\" լեզուն %2$S­ից տեղադրել:"

#: OK
msgid "Install"
msgstr "Տեղադրել"

#: progress.queued
msgid "Queued"
msgstr "Հերթի մեջ"

#: progress.downloading
msgid "Downloading…"
msgstr "Բեռնավորում եմ…"

#: progress.downloaded
msgid "Downloaded"
msgstr "Բեռնավորված է"

#: progress.installing
msgid "Installing…"
msgstr "Տեղակայում..."

#: Unsigned
msgid "Unsigned"
msgstr "Ստորագրված չէ"

#. #--------------------------------------------------------------------
#. #   Miscellaneous
#. #--------------------------------------------------------------------
#: ERROR
msgid "ERROR"
msgstr "ՍԽԱԼՄՈՒՆՔ"

#: error0
msgid "Success"
msgstr "Հաջողություն"

#: error999
msgid "Restart to complete"
msgstr "Եզրափակելու համար վերագործարկեք"

#: error-202
msgid "Access denied"
msgstr "Արգելված գործողություն"

#: error-203
msgid ""
"Unexpected installation error\n"
"Review the Error Console log for more details."
msgstr ""
"Տեղադրման անսպասելի սխալմունք\n"
"Մանրամասների համար քննեք Սխալների Կոնսոլի Գրառումները:"

#: error-204
msgid "Install script not found"
msgstr "Տեղադրման սկրիպտը չի գտնվել"

#: error-207
msgid "Not a valid install package"
msgstr "Տեղադրման փաթեթն անվավեր է"

#: error-208
msgid "Invalid argument"
msgstr "Անվավեր փաստարկ"

#: error-210
msgid "User canceled"
msgstr "Դադարեցվեց օգտվողի կողմից"

#: error-214
msgid "Required file does not exist"
msgstr "Անհրաժեշտ ֆայլն առկա չէ"

#: error-215
msgid "Read only"
msgstr "Չփոփոխվող"

#: error-218
msgid "AppleSingle extraction error"
msgstr "AplleSingle  զատման սխալմունք"

#: error-219
msgid "Invalid path"
msgstr "Անվավեր ծրագիծ"

#: error-225
msgid "EXTRACTION_FAILED"
msgstr "Զատումը Խափանվեց"

#: error-227
msgid "Canceled"
msgstr "Դադարեցված"

#: error-228
msgid "Download error"
msgstr "Բեռնման վթար"

#: error-229
msgid "Script error"
msgstr "Սկրիպտի սխալմունք"

#: error-230
msgid "Already exists"
msgstr "Արդեն Առկա է"

#: error-235
msgid "Out of space"
msgstr "Տեղ չկա"

#: error-239
msgid "Chrome registration failed"
msgstr "Chrome գրանցումը խափանվեց"

#: error-240
msgid "Unfinished install"
msgstr "Անավարտ տեղադրում"

#: error-244
msgid "Unsupported package"
msgstr "Անսպասարկելի փաթեթ"

#: error-260
msgid "Signing could not be verified."
msgstr "Անհնար էր ստուգել ստորագրությունը:"

#: error-261
msgid "Invalid file hash (possible download corruption)"
msgstr "Ֆայլի հաշն անվավեր է (հնարավոր բեռնավորման խոտան)"

#: error-262
msgid "Unknown or invalid file hash type"
msgstr "Անհայտ կամ անվավեր հաշի տեսակ"

#: error-299
msgid "Out of memory"
msgstr "Հիշողության չափազանցում"

#. # there are other error codes, either rare or obsolete,
#. # that are not worth translating at this time.
#: unknown.error
msgid "Unexpected error %S"
msgstr "Անհայտ սխալմունք %S"
