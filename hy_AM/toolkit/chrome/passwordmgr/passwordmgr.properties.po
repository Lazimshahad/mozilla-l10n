#. # LOCALIZATION NOTE (updatePasswordText):
#. # String is the username for the login.
#: updatePasswordText
msgid "Update saved password for \"%S\"?"
msgstr "Թարմացնե՞լ \"%S\"-ի համար պահված գաղտնաբառը:"

#: updatePasswordTextNoUser
msgid "Update saved password?"
msgstr "Թարմացնե՞լ պահված գաղտնաբառը:"

#: notifyBarUpdateButtonText
msgid "Update Password"
msgstr "Թարմացնել գաղտնաբառը"

#: notifyBarUpdateButtonAccessKey
msgid "U"
msgstr "U"

#: notifyBarDontChangeButtonText
msgid "Don't Change"
msgstr "Չփոխել"

#: notifyBarDontChangeButtonAccessKey
msgid "D"
msgstr "D"

#: userSelectText
msgid "Please confirm which user you are changing the password for"
msgstr "Խնդրեմ նշեք թե որ օգտվողի գաղտնաբառն էք փոխում"

#: hidePasswords
msgid "Hide Passwords"
msgstr "Թաքցնել Գաղտնաբառերը"

#: hidePasswordsAccessKey
msgctxt "hidePasswordsAccessKey"
msgid "P"
msgstr "P"

#: showPasswords
msgid "Show Passwords"
msgstr "Ցուցադրել Գաղտնաբառերը"

#: showPasswordsAccessKey
msgctxt "showPasswordsAccessKey"
msgid "P"
msgstr "P"

#: noMasterPasswordPrompt
msgid "Are you sure you wish to show your passwords?"
msgstr "Իրո՞ք ցանկանում եք Ձեր գաղտնաբառերը ցուցադրել:"

#: removeAllPasswordsPrompt
msgid "Are you sure you wish to remove all passwords?"
msgstr "Դուք համոզված էք որ ցանկանում էք ջնջել բոլոր գաղտնաբառերը?"

#: removeAllPasswordsTitle
msgid "Remove all passwords"
msgstr "Ջնջել բոլոր գաղտնաբառերը"

#: loginsSpielAll
msgid "Passwords for the following sites are stored on your computer:"
msgstr "Այս կայքի գաղտնաբառերը պահված են ձեր համակարգչում:"

#: loginsSpielFiltered
msgid "The following passwords match your search:"
msgstr "Հետևյալ գաղտնաբառը համապատասխանում է ձեր որոնմանը:"
