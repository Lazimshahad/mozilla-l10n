#. # Map Expat error codes to error strings
#: 1
msgid "out of memory"
msgstr "egwaniika ewendemu"

#: 2
msgid "syntax error"
msgstr "ensobi mu matteka"

#: 3
msgid "no element found"
msgstr "ekintu tekizuuse"

#: 4
msgid "not well-formed"
msgstr "tekitegerekese"

#: 5
msgid "unclosed token"
msgstr "tokeni nga teggale"

#: 6
msgid "partial character"
msgstr "ebitoono kumpiisa"

#: 7
msgid "mismatched tag"
msgstr "ta'aga ezitakwatagana"

#: 8
msgid "duplicate attribute"
msgstr "ebisanyizzo nga bididdwamu"

#: 9
msgid "junk after document element"
msgstr "bwiino nga tekakase oluvanyuma lwe wandiiko kye kintu"

#: 10
msgid "illegal parameter entity reference"
msgstr "obujjulizi bwe kintu nga te bukkirizirwa"

#: 11
msgid "undefined entity"
msgstr "obusanyizo obutategerekanyi"

#: 12
msgid "recursive entity reference"
msgstr "okujuliza kwa kantu okweddamu"

#: 13
msgid "asynchronous entity"
msgstr "tekikwatagana ekintu"

#: 14
msgid "reference to invalid character number"
msgstr "ojjuliza enukuta ezi takkirizirwa"

#: 15
msgid "reference to binary entity"
msgstr "jjuliza ekintu mu bibiri"

#: 16
msgid "reference to external entity in attribute"
msgstr "jjuliza ebisanyirizo bya kantu ebyomunda"

#: 17
msgid "XML or text declaration not at start of entity"
msgstr "XML oba ebigambo ekimayisirwa tikitandiika obunyiso"

#: 18
msgid "unknown encoding"
msgstr "ekweka etategerekanyi"

#: 19
msgid "encoding specified in XML declaration is incorrect"
msgstr "okukweka mu XML etegeza kuffu"

#: 20
msgid "unclosed CDATA section"
msgstr "ekitundu kya CDATA nga tekiggale"

#: 21
msgid "error in processing external entity reference"
msgstr "ensobi mu kukolawo obujjulizi bwa kantu kobweru"

#: 22
msgid "document is not standalone"
msgstr "ekiwandiiko tekola kyoka"

#: 23
msgid "unexpected parser state"
msgstr "akagatta ekitasubirwa ebeera"

#: 24
msgid "entity declared in parameter entity"
msgstr "obumayiso obuttegezzirwa muddagiriro y`obumayiso"

#: 27
msgid "prefix not bound to a namespace"
msgstr "soka g`tozibizirwa ku linnya le banga"

#: 28
msgid "must not undeclare prefix"
msgstr "tolina kuttegeza ebisokela"

#: 29
msgid "incomplete markup in parameter entity"
msgstr "ebitamalirizidwa kulabwako kubumaso obuwedwa"

#: 30
msgid "XML declaration not well-formed"
msgstr "XML ettegezziddwa tekoledwa bulungi"

#: 31
msgid "text declaration not well-formed"
msgstr "ekkiwandiko ekittegezidwa tekoledwa bulungi"

#: 32
msgid "illegal character(s) in public id"
msgstr "ennukuta(s) ezitaliiyo manya ku lukale"

#: 38
msgid ""
"reserved prefix (xml) must not be undeclared or bound to another namespace "
"name"
msgstr ""
"ezziterekirwa zisokeddwawo(xml) tezirina kuttegezidwa oba ku zibiziddwa ku "
"namespace ya erinya"

#: 39
msgid "reserved prefix (xmlns) must not be declared or undeclared"
msgstr ""
"ezziterekirwa zisokeddwawo(xmlns) tezirina kuttegezidwa oba te`ttegeziddwa"

#: 40
msgid "prefix must not be bound to one of the reserved namespace names"
msgstr ""
"ebisokeddwawo tebirina okuzibikkiddwa ku kimu ekitterekeddwa mu banga la "
"amanya"

#. # %1$S is replaced by the Expat error string, may be followed by Expected (see below)
#. # %2$S is replaced by URL
#. # %3$u is replaced by line number
#. # %4$u is replaced by column number
#: XMLParsingError
msgid ""
"XML Parsing Error: %1$S\n"
"Location: %2$S\n"
"Line Number %3$u, Column %4$u:"
msgstr ""
"Ekisobu mu kuwerezeganya kwa XML: %1$S\n"
"Wekiri: %2$S\n"
"Ennamba yalayini %3$u, Omusimbalala %4$u:"

#. # %S is replaced by a tag name.
#. # This gets appended to the error string if the error is mismatched tag.
#: Expected
msgid ". Expected: </%S>."
msgstr ". Ekisubirwa: </%S>."

#~ msgid "xml processing instruction not at start of external entity"
#~ msgstr "xml processing instruction not at start of external entity"
