#. Don't localize these. They're here until they have
#. a better place to live.
#: syncBrand.fullName.label
msgid "Firefox Sync"
msgstr "Okutusaganya Firefox"

#: syncBrand.shortName.label
msgid "Sync"
msgstr "Tusaganya"

#. Main titles.
#: sync.app.name.label
msgctxt "sync.app.name.label"
msgid "&syncBrand.fullName.label;"
msgstr "&syncBrand.fullName.label;"

#: sync.title.connect.label
msgid "Connect to &syncBrand.shortName.label;"
msgstr "Yunga ku &syncBrand.shortName.label;"

#: sync.title.adddevice.label
msgid "Add a &syncBrand.fullName.label; Account"
msgstr "Youngera Account ya &syncBrand.fullName.label;"

#: sync.title.pair.label
msgid "Pair a Device"
msgstr "Yunga Ekyuma"

#. J-PAKE Key Screen
#: sync.subtitle.connect.label
msgid ""
"To activate your new device, select “Set up &syncBrand.shortName.label;” on "
"the device."
msgstr ""
"Okutandika ekyuma ekipya, londa \"Tegeka &syncBrand.shortName.label;” ku "
"kyuma."

#: sync.subtitle.header.label
msgid "Enter this code on your computer"
msgstr "Yingiza kodi eno mu kumpyuta yo"

#: sync.subtitle.connectlocation.label
msgid ""
"Select “&sync.title.pair.label;” in the &syncBrand.shortName.label; section "
"of your desktop Firefox options."
msgstr ""
"Londa “&sync.title.pair.label;” mu kitundu kya &syncBrand.shortName.label; "
"ekye bilondebwako by Firefox."

#: sync.subtitle.pair.label
msgid "To activate, select “Pair a device” on your other device."
msgstr "Okutandika, londa \"Yunga a ekyuma\" ku kyuma kyo ekirala."

#: sync.pin.default.label
msgid "...\\n...\\n...\\n"
msgstr "...\\n...\\n...\\n"

#: sync.pin.oneline.label
msgid "..."
msgstr "..."

#: sync.link.show.label
msgid "Show me how."
msgstr "Ndaga nga."

#: sync.link.advancedsetup.label
msgid "Advanced setup…"
msgstr "Okutegeka Okwawagulu..."

#: sync.link.nodevice.label
msgid "I don\\'t have the device with me…"
msgstr "Sirina ekyuma wano nange..."

#. J-PAKE Waiting Screen
#: sync.jpake.subtitle.waiting.label
msgid "Waiting for other device…"
msgstr "Okulindirira ekyuma ekyirala…"

#. Account Login Screen
#: sync.subtitle.account.label
msgid "Enter your &syncBrand.fullName.label; account information"
msgstr "Tekamu ebikukwata ko mu akaunta yo eya &syncBrand.fullName.label;"

#: sync.input.username.label
msgid "Account Name"
msgstr "Elinya lya Akaunti"

#: sync.input.password.label
msgid "Password"
msgstr "Ekisumuluzo"

#: sync.input.key.label
msgid "Recovery Key"
msgstr "Akasumuluzo Kange Akazaobujja"

#: sync.checkbox.server.label
msgid "Use custom server"
msgstr "Kozesa engabizi jjewetegekedde"

#: sync.input.server.label
msgid "Server URL"
msgstr "URL Yengabizi"

#. Setup Fail
#: sync.title.fail.label
msgid "Cannot Set Up &syncBrand.shortName.label;"
msgstr "Tekisoboka okutegeka &syncBrand.shortName.label;"

#: sync.subtitle.fail.label
msgid ""
"&syncBrand.fullName.label; could not connect to the server. Would you like "
"to try again?"
msgstr ""
"&syncBrand.fullName.label; tesoobode kuyunga ku ngabizi. Wandiyagade "
"okugezako nate?"

#: sync.button.tryagain.label
msgid "Try again"
msgstr "Ddamu ogezeeko"

#: sync.button.manual.label
msgid "Manual Setup"
msgstr "Okwetegekera"

#: sync.subtitle.nointernet.label
msgid "No internet connection available."
msgstr "Tewali mukutu gwa yintanet guliwo."

#. Setup Success
#: sync.title.success.label
msgid "Setup Complete"
msgstr "Okutekamu Kuwede"

#: sync.subtitle.success.label
msgid ""
"Your data is now being downloaded in the background. You can go to Settings "
"to manage your account."
msgstr ""
"Bwino wo kati awanulwa emabega. Oyinza okugenda muntegeka oku dukanya "
"akaunta yo."

#: sync.settings.label
msgid "Settings"
msgstr "Enteegeka"

#: sync.subtitle.manage.label
msgid ""
"Your &syncBrand.fullName.label; account is already set up. Go to Settings to "
"manage your account."
msgstr ""
"Akaunta yo eya &syncBrand.fullName.label; ewede okutegeka. Genda mu ntegeka "
"okudukanya akaunta yo."

#. Pair Device
#: sync.pair.tryagain.label
msgid "Please try again."
msgstr "Bambi gezako nate."

#. Firefox SyncAdapter Settings Screen
#: sync.settings.options.label
msgid "Options"
msgstr "Okweroboza"

#: sync.summary.pair.label
msgid "Link another device to your &syncBrand.shortName.label; account"
msgstr "Yunga ekyuma ekirala ku akaunta yo eya &syncBrand.shortName.label;"

#. Common text
#: sync.button.cancel.label
msgid "Cancel"
msgstr "Sazaamu"

#: sync.button.connect.label
msgid "Connect"
msgstr "Yunga"

#: sync.button.ok.label
msgid "OK"
msgstr "OK"

#. Account strings
#: sync.account.label.label
msgctxt "sync.account.label.label"
msgid "&syncBrand.fullName.label;"
msgstr "&syncBrand.fullName.label;"

