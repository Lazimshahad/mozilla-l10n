msgstr "Ennaku(s)"

#: validation.crl.autoupdate.url.label
msgid "CRL would be imported From:"
msgstr "CRL ejjaku jjirwa Kuva:"

#: crl.import.status.title
msgid "CRL Import Status"
msgstr "CRL ejjaku Jjirwa ku bera"

#: crl.import.success.message
msgid "The Certificate Revocation List (CRL) was successfully imported."
msgstr ""
"Olukalala rwa Satifikati ezitaliyo (CRL) rwa jjirwa kuva awalala burugi."

#: crl.issuer.label
msgid "CRL Issued By:"
msgstr "CRL Ewerendwa Na:"

