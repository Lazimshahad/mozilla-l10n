#: MimeNotCss
msgid ""
"The stylesheet %1$S was not loaded because its MIME type, \"%2$S\", is not "
"\"text/css\"."
msgstr ""
"O stylesheet %1$S no l'é stæto caregòu perché o seu tîpo MIME, \"%2$S\", o "
"no l'é \"text/css\"."

#: MimeNotCssWarn
msgid ""
"The stylesheet %1$S was loaded as CSS even though its MIME type, \"%2$S\", "
"is not \"text/css\"."
msgstr ""
"O stylesheet %1$S no l'é stæto caregòu cómme CSS perché o seu tîpo MIME, "
"\"%2$S\", o no l'é \"text/css\"."

#: PEUnexpEOF2
msgid "Unexpected end of file while searching for %1$S."
msgstr "No m'aspetâvo a fìn do file fintànto che çercâvo %1$S."

#: PEParseRuleWSOnly
msgid "Whitespace-only string given to be parsed as rule."
msgstr "Ti m'æ dæto 'na stringa da anlizâ ch'a l'é fæta da sôli spàççi."

#: PEDeclDropped
msgid "Declaration dropped."
msgstr "Diciaraçión scancelâ."

#: PEDeclSkipped
msgid "Skipped to next declaration."
msgstr "Sâto a pròscima diciaraçión."

#: PEUnknownProperty
msgid "Unknown property '%1$S'."
msgstr "Propietæ sconosciûa '%1$S'."

#: PEValueParsingError
msgid "Error in parsing value for '%1$S'."
msgstr "Erô inte l'anàlixi do valô pe '%1$S'."

#: PEExpectEndValue
msgid "Expected end of value but found '%1$S'."
msgstr "M'aspetâvo a fìn do valô ma ò trovòu '%1$S'."

#: PESkipAtRuleEOF
msgid "end of unknown at-rule"
msgstr "fìn do unknown at-rule"

#: PEUnknownAtRule
msgid "Unrecognized at-rule or error parsing at-rule '%1$S'."
msgstr "Rególa no conosciûa ò erô inte l'anàlixi da rególa '%1$S'."

#: PECharsetRuleEOF
msgid "charset string in @charset rule"
msgstr "charset string inta régola @charset"

#: PECharsetRuleNotString
msgid "Expected charset string but found '%1$S'."
msgstr "M'aspetâvo 'na stringa ma ò trovòu '%1$S'."

#: PEGatherMediaEOF
msgid "end of media list in @import or @media rule"
msgstr "fìn da lìsta di media inta régola @import ò @media"

#: PEGatherMediaNotComma
msgid "Expected ',' in media list but found '%1$S'."
msgstr "M'aspetâvo ',' inta lìsta di media ma ò trovòu '%1$S'."

#: PEGatherMediaNotIdent
msgid "Expected identifier in media list but found '%1$S'."
msgstr "M'aspetâvo 'n identificatô inta lìsta di media ma ò trovòu '%1$S'."

#: PEImportNotURI
msgid "Expected URI in @import rule but found '%1$S'."
msgstr "M'aspetâvo URI inta régola @import ma ò trovòu '%1$S'."

#: PEImportBadURI
msgid "Invalid URI in @import rule: '%1$S'."
msgstr "URI no vàlido inta rególa @import: '%1$S'."

#: PEImportUnexpected
msgid "Found unexpected '%1$S' within @import."
msgstr "No m'aspetâvo '%1$S' con @import."

#: PEGroupRuleEOF
msgid "end of @media or @-moz-document rule"
msgstr "fìn do @media ò da régola @-moz-document"

#: PEGroupRuleNestedAtRule
msgid "%1$S rule not allowed within @media or @-moz-document rule."
msgstr "A régola %1$S a no l'é permìssa co-a régola @media ò @-moz-document."

#: PEMozDocRuleBadFunc
msgid ""
"Expected url(), url-prefix(), or domain() in @-moz-document rule but found "
"'%1$S'."
msgstr ""
"M'aspetâvo url(), url-prefix(), ò domain() inta régola @-moz-document ma ò "
"trovòu '%1$S'."

#: PEMozDocRuleNotURI
msgid "Expected URI in @-moz-document rule but found '%1$S'."
msgstr "M'aspetâvo URI inta régola @-moz-document ma ò trovòu '%1$S'."

#: PEMozDocRuleNotString
msgid ""
"Expected string in @-moz-document rule regexp() function but found '%1$S'."
msgstr ""
"M'aspetâvo string inta régola @-moz-document fonçión regexp() ma ò trovòu "
"'%1$S'."

#: PEMozDocRuleEOF
msgid "next URI in @-moz-document rule"
msgstr "next URI inta régola @-moz-document"

#: PEAtNSPrefixEOF
msgid "namespace prefix in @namespace rule"
msgstr "prefìsso do namespace inta régola @namespace"

#: PEAtNSURIEOF
msgid "namespace URI in @namespace rule"
msgstr "URI do namespace inta régola @namespace"

#: PEAtNSUnexpected
msgid "Unexpected token within @namespace: '%1$S'."
msgstr "No m'aspetâvo sto token con @namespace: '%1$S'."

#: PEKeyframeNameEOF
msgid "name of @keyframes rule."
msgstr "nómme da régola @keyframes."

#: PEKeyframeBadName
msgid "Expected identifier for name of @keyframes rule."
msgstr "M'aspetâvo 'n'identificatô pe-o nómme da régola @keyframes."

#: PEKeyframeBrace
msgid "Expected opening { of @keyframes rule."
msgstr "M'aspetâvo { da régola @keyframes."

#: PESkipDeclBraceEOF
msgid "closing } of declaration block"
msgstr "særo } do blòcco da diciaraçión"

#: PESkipRSBraceEOF
msgid "closing } of invalid rule set"
msgstr "særo } de 'na régola no vàlida"

#: PEBadSelectorRSIgnored
msgid "Ruleset ignored due to bad selector."
msgstr "Set de régole ignoræ a càoza de 'n seleçionatô catîvo."

#: PEBadSelectorKeyframeRuleIgnored
msgid "Keyframe rule ignored due to bad selector."
msgstr "Régola keyframe ignorâ a caoza de 'n seletô catîvo."

#: PESelectorListExtraEOF
msgid "',' or '{'"
msgstr "',' ò '{'"

#: PESelectorListExtra
msgid "Expected ',' or '{' but found '%1$S'."
msgstr "M'aspetâvo ',' ò '{' ma ò trovòu '%1$S'."

#: PESelectorGroupNoSelector
msgid "Selector expected."
msgstr "M'aspêto o seletô."

#: PESelectorGroupExtraCombinator
msgid "Dangling combinator."
msgstr "Conbinatô ch'o lòccia."

#: PEClassSelEOF
msgid "class name"
msgstr "nómme da clàsse"

#: PEClassSelNotIdent
msgid "Expected identifier for class selector but found '%1$S'."
msgstr "M'aspetâvo 'n'identificatô pe-a clàsse selector ma ò trovòu '%1$S'."

#: PETypeSelEOF
msgid "element type"
msgstr "tîpo de l'eleménto"

#: PETypeSelNotType
msgid "Expected element name or '*' but found '%1$S'."
msgstr "M'aspetâvo o nómme de l'eleménto ò '*' ma ò trovòu '%1$S'."

#: PEUnknownNamespacePrefix
msgid "Unknown namespace prefix '%1$S'."
msgstr "Prefìsso '%1$S' sconosciûo."

#: PEAttributeNameEOF
msgid "attribute name"
msgstr "nómme de l'atribûto"

#: PEAttributeNameExpected
msgid "Expected identifier for attribute name but found '%1$S'."
msgstr ""
"M'aspetâvo 'n'identificatô pe-o nómme de l'atribûto ma ò trovòu '%1$S'."

#: PEAttributeNameOrNamespaceExpected
msgid "Expected attribute name or namespace but found '%1$S'."
msgstr "M'aspetâvo o nómme de l'atribûto ò do namespace ma ò trovòu '%1$S'."

#: PEAttSelNoBar
msgid "Expected '|' but found '%1$S'."
msgstr "M'aspetâvo '|' ma ò trovòu '%1$S'."

#: PEAttSelInnerEOF
msgid "part of attribute selector"
msgstr "pàrte do seletô de l'atribûto"

#: PEAttSelUnexpected
msgid "Unexpected token in attribute selector: '%1$S'."
msgstr "No m'aspetâvo o token into seletô de l'atribûto: '%1$S'."

#: PEAttSelValueEOF
msgid "attribute value"
msgstr "valô de l'atribûto"

#: PEAttSelCloseEOF
msgid "']' to end attribute selector"
msgstr "']' a-a fìn de 'n'atribûto seletô"

#: PEAttSelNoClose
msgid "Expected ']' to terminate attribute selector but found '%1$S'."
msgstr "M'aspetâvo ']' pe terminâ o seletô de l'atribûto ma ò trovòu '%1$S'."

#: PEAttSelBadValue
msgid ""
"Expected identifier or string for value in attribute selector but found "
"'%1$S'."
msgstr ""
"M'aspetâvo 'n'identificatô ò stringa cómme valô inte seletô de l'atribûto ma "
"ò trovòu '%1$S'."

#: PEPseudoSelEOF
msgid "name of pseudo-class or pseudo-element"
msgstr "nómme da pseodo-classe ò pseodo-eleménto"

#: PEPseudoSelBadName
msgid ""
"Expected identifier for pseudo-class or pseudo-element but found '%1$S'."
msgstr "M'aspetâvo 'n'identificatô pe-a pseodo-clàsse ma ò trovòu '%1$S'."

#: PEPseudoSelNonFunc
msgid ""
"Function token for non-function pseudo-class or pseudo-element, or the other "
"way around, when reading '%1$S'."
msgstr ""
"Fonçión token pe-a pseodo-clàsse ò pseodo-eleménto no-fonçión, ò in âtro "
"mòddo, quànde lezêvo '%1$S'."

#: PEPseudoSelNotPE
msgid "Expected pseudo-element but found '%1$S'."
msgstr "M'aspetâvo o pseodo-eleménto ma ò trovòu '%1$S'."

#: PEPseudoSelDoubleNot
msgid "Negation pseudo-class can't be negated '%1$S'."
msgstr "Negaçión da pseodo-clàsse ma no se peu negâ '%1$S'."

#: PEPseudoSelPEInNot
msgid "Pseudo-elements can't be negated '%1$S'."
msgstr "I pseodo-eleménti no peuan êse negæ '%1$S'."

#: PEPseudoSelNewStyleOnly
msgid "This pseudo-element must use the \"::\" form: '%1$S'."
msgstr "Sto pseodo-eleménto o deve adêuvia a \"::\" form: '%1$S'."

#: PEPseudoSelTrailing
msgid ""
"Found trailing token after pseudo-element, which must be the last part of a "
"selector:  '%1$S'."
msgstr ""
"Trovòu 'n token dòppo o pseodo-eleménto, che deve êse l'ùrtima pàrte de 'n "
"seletô: '%1$S'."

#: PEPseudoSelMultiplePE
msgid "Extra pseudo-element '%1$S'."
msgstr "Extra pseodo-eleménto '%1$S'."

#: PEPseudoSelUnknown
msgid "Unknown pseudo-class or pseudo-element '%1$S'."
msgstr "Pseodo-clàsse ò pseodo-eleménto sconosciûo '%1$S'."

#: PENegationEOF
msgid "selector within negation"
msgstr "seletô con negaçión"

#: PENegationBadInner
msgid "Malformed simple selector as negation pseudo-class argument '%1$S'."
msgstr "Seletô sénplice mâ fæto cómme argoménto da pseodo-clàsse negâ '%1$S'."

#: PENegationNoClose
msgid "Missing closing ')' in negation pseudo-class '%1$S'."
msgstr "Mànca ')' inta pseodo-clàsse negâ '%1$S'."

#: PENegationBadArg
msgid "Missing argument in negation pseudo-class '%1$S'."
msgstr "Mànca 'n'argoménto inta pseodo-clàsse negâ '%1$S'."

#: PEPseudoClassArgEOF
msgid "argument to pseudo-class selector"
msgstr "argoménto de 'n seletô de 'na pseodo-clàsse"

#: PEPseudoClassArgNotIdent
msgid "Expected identifier for pseudo-class parameter but found '%1$S'."
msgstr "M'aspetâvo 'n'identificatô pe-a pseodo-clàsse ma ò trovòu '%1$S'."

#: PEPseudoClassArgNotNth
msgid "Expected part of argument to pseudo-class but found '%1$S'."
msgstr ""
"M'aspetætâvo a pàrte de l'argoménto pe-a pseodo-clàsse ma ò trovòu '%1$S'."

#: PEPseudoClassNoClose
msgid "Missing closing ')' in pseudo-class, found '%1$S' instead."
msgstr "Mànca a ')' pe serâ a pseodo-clàsse, ma ò trovòu '%1$S' in càngio."

#: PEPseudoClassNoArg
msgid "Missing argument in pseudo-class '%1$S'."
msgstr "M'aspetâvo 'n'argoménto inta pseodo-clàsse '%1$S'."

#: PESelectorEOF
msgid "selector"
msgstr "seletô"

#: PEBadDeclBlockStart
msgid "Expected '{' to begin declaration block but found '%1$S'."
msgstr "M'aspetâvo '{' pe iniçiâ o blòcco da diciaraçión ma ò trovòu '%1$S'."

#: PEColorEOF
msgid "color"
msgstr "cô"

#: PEColorNotColor
msgid "Expected color but found '%1$S'."
msgstr "M'aspetâvo o cô ma ò trovòu '%1$S'."

#: PEColorComponentEOF
msgid "color component"
msgstr "conponénte do cô"

#: PEExpectedPercent
msgid "Expected a percentage but found '%1$S'."
msgstr "M'aspetâvo 'na percentoale ma ò trovòu '%1$S'."

#: PEExpectedInt
msgid "Expected an integer but found '%1$S'."
msgstr "M'aspetâvo 'n'integer ma ò trovòu '%1$S'."

#: PEColorBadRGBContents
msgid "Expected number or percentage in rgb() but found '%1$S'."
msgstr "M'aspetâvo 'n  nùmero ò percentoale in rgb() ma ò trovòu '%1$S'."

#: PEColorComponentBadTerm
msgid "Expected '%2$S' but found '%1$S'."
msgstr "M'aspetâvo '%2$S' ma ò trovòu '%1$S'."

#: PEColorHueEOF
msgid "hue"
msgstr "cô"

#: PEExpectedComma
msgid "Expected ',' but found '%1$S'."
msgstr "M'aspetâvo ',' ma ò trovòu '%1$S'."

#: PEColorSaturationEOF
msgid "saturation"
msgstr "saturaçión"

#: PEColorLightnessEOF
msgid "lightness"
msgstr "lumininozitæ"

#: PEColorOpacityEOF
msgid "opacity in color value"
msgstr "òpacitæ into valô do cô"

#: PEExpectedNumber
msgid "Expected a number but found '%1$S'."
msgstr "M'aspetâvo un nûmero ma ò trovòu '%1$S'."

#: PEExpectedCloseParen
msgid "Expected ')' but found '%1$S'."
msgstr "M'aspetâvo ')' ma ò trovòu '%1$S'."

#: PEDeclEndEOF
msgid "';' or '}' to end declaration"
msgstr "';' ò '}' a-a fìn da diciaraçión"

#: PEParseDeclarationNoColon
msgid "Expected ':' but found '%1$S'."
msgstr "M'aspetâvo ':' ma ò trovòu '%1$S'."

#: PEParseDeclarationDeclExpected
msgid "Expected declaration but found '%1$S'."
msgstr "M'aspetâvo 'na diciaraçión ma ò trovòu '%1$S'."

#: PEEndOfDeclEOF
msgid "end of declaration"
msgstr "fìn da diciaraçión"

#: PEImportantEOF
msgid "important"
msgstr "inportànte"

#: PEExpectedImportant
msgid "Expected 'important' but found '%1$S'."
msgstr "M'aspetâvo 'important' ma ò trovòu '%1$S'."

#: PEBadDeclEnd
msgid "Expected ';' to terminate declaration but found '%1$S'."
msgstr "M'aspetâvo ';' pe terminâ a diciaraçión ma ò trovòu '%1$S'."

#: PEBadDeclOrRuleEnd2
msgid "Expected ';' or '}' to terminate declaration but found '%1$S'."
msgstr "M'aspetâvo ';' ò '}' pe terminâ a diciaraçión ma ò trovòu '%1$S'."

#: PEInaccessibleProperty2
msgid "Cannot specify value for internal property."
msgstr "No pòsso specificâ 'n valô pe a propietæ intèrna."

#: PECommentEOF
msgid "end of comment"
msgstr "fìn do coménto"

#: SEUnterminatedString
msgid "Found unclosed string '%1$S'."
msgstr "Atrovâ 'na stringa no serâ '%1$S'."

#: PEFontDescExpected
msgid "Expected font descriptor but found '%1$S'."
msgstr "M'aspetâvo 'n desritô do caràtere ma ò trovòu '%1$S'."

#: PEUnknownFontDesc
msgid "Unknown descriptor '%1$S' in @font-face rule."
msgstr "Descritô sconosciûo '%1$S' inta règola @font-face."

#: PEMQExpectedExpressionStart
msgid "Expected '(' to start media query expression but found '%1$S'."
msgstr ""
"M'aspetâvo '(' pe iniçiâ l'espresción da query media ma ò trovòu '%1$S'."

#: PEMQExpressionEOF
msgid "contents of media query expression"
msgstr "contegnûo de l'espersción media query"

#: PEMQExpectedFeatureName
msgid "Expected media feature name but found '%1$S'."
msgstr "M'aspetâvo 'n nómme de 'na caràteristica media ma ò trovòu '%1$S'."

#: PEMQExpectedFeatureNameEnd
msgid "Expected ':' or ')' after media feature name but found '%1$S'."
msgstr ""
"M'aspetâvo ':' ò ')' dòppo o nómme da caràteristica media ma ò trovòu '%1$S'."

#: PEMQNoMinMaxWithoutValue
msgid "Media features with min- or max- must have a value."
msgstr "Càrateristica media con min- ò max- deve avéi 'n valô."

#: PEMQExpectedFeatureValue
msgid "Found invalid value for media feature."
msgstr "Ò trovòu 'n valô no vàlido pe-a caràteristica media."

#: PEBadFontBlockStart
msgid "Expected '{' to begin @font-face rule but found '%1$S'."
msgstr "M'aspetâvo '{' pe iniçiâ @font-face ma ò trovòu '%1$S'."

#: PEBadFontBlockEnd
msgid "Expected '}' to end @font-face rule but found '%1$S'."
msgstr "M'aspetâvo '}' pe finî @font-face ma ò trovòu '%1$S'."

#: PEAnonBoxNotAlone
msgid "Did not expect anonymous box."
msgstr "No se aspêtavâ 'na scàtoa anònima."
