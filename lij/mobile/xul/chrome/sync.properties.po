#. # Mobile Sync
#. # %S is the date and time at which the last sync successfully completed
#: lastSync2.label
msgid "Last sync: %S"
msgstr "Ùrtima scincronizaçion: %S"

#: lastSyncInProgress2.label
msgid "Last sync: in progress…"
msgstr "Ùrtima sincronizaçión: a sta anàndo"

#. # %S is the username logged in
#: account.label
msgid "Account: %S"
msgstr "Account: %S"

#: notconnected.label
msgid "Not connected"
msgstr "No conésso"

#: connecting.label
msgid "Connecting…"
msgstr "Conétto…"

#: notificationDisconnect.label
msgid "Your Firefox Sync account has been removed"
msgstr "O teu account Firefox Sync o l'é stætp scancelòu"

#: notificationDisconnect.button
msgid "Undo"
msgstr "Anùlla"

#. # LOCALIZATION NOTE (sync.clientUpdate, sync.remoteUpdate):
#. # #1 is the "application name"
#. # #2 is the "version"
#: sync.update.client
msgid ""
"#1 #2 is not compatible with the latest version of Firefox Sync. Please "
"update to the latest version."
msgstr ""
"#1 #2 o no l'é conpatìbile con l'ùrtima versción de Firefox sync. Pe favô "
"agiórnite a l'ùrtima versción."

#: sync.update.remote
msgid ""
"#1 #2 is not compatible with older versions of Firefox Sync. Please update "
"Firefox on your other computer(s)."
msgstr ""
"#1 #2 o no l'é conpatìbile con e versción vêge de Firefox sync. Pe favô "
"agiórna Firefox in sci tùtti i teu âtri computer."

#: sync.update.title
msgid "Firefox Sync"
msgstr "Firefox Sync"

#: sync.update.button
msgid "Learn More"
msgstr "Pe savéine de ciù"

#: sync.update.close
msgid "Close"
msgstr "Særa"

#: sync.setup.error.title
msgid "Cannot Setup Sync"
msgstr "No pòsso inpostâ o Sync"

#: sync.setup.error.network
msgid "No internet connection available"
msgstr "No gh'é a conesciòn internét"

#: sync.setup.error.nodata
msgid "%S could not connect to Sync. Would you like to try again?"
msgstr "O %S o no se peu conétte a Sync. Ti veu mîga provâ tórna?"

#: sync.setup.tryagain
msgid "Try again"
msgstr "Prêuva ancón"

#: sync.setup.manual
msgid "Manual setup"
msgstr "Inpostaçión a màn"

#: sync.message.notabs
msgid "No tabs from your other computers."
msgstr "Nisciùn féuggio da-i âtri computers."
