msgstr ""

#: reloadPlugin.middle
msgid "Reload the page"
msgstr "Recarega pàgina"

#: reloadPlugin.post
msgid " to try again."
msgstr " pe provâ ancón."

#. This and the other report.* strings should be as short as possible, ideally 2-3 words.
#: report.please
msgid "Send crash report"
msgstr "Mànda 'na segnalaçión"

#: report.submitting
msgid "Sending report…"
msgstr "Spedìscio o report…"

#: report.submitted
msgid "Crash report sent."
msgstr "Report do problêma spediô."

#: report.disabled
msgid "Crash reporting disabled."
msgstr "Regìstro di problêmi dizabilitòu."

#: report.failed
msgid "Submission failed."
msgstr "Spediçión no arioscîa."

#: report.unavailable
msgid "No report available."
msgstr "No gh'é nisciùn report."
