msgstr "< &Morago"

#: NextBtn
msgid "&Next >"
msgstr "&Latelago >"

#: AcceptBtn
msgid "I &accept the terms in the License Agreement"
msgstr "Ke &amogela mabaka a tumelelano ya laesense"

#: DontAcceptBtn
msgid "I &do not accept the terms in the License Agreement"
msgstr "Ga &ke amogele mabaka a tumelelo ya laesense"

#: InstallBtn
msgid "&Install"
msgstr "&Lokela"

#: UninstallBtn
msgid "&Uninstall"
msgstr "&Ntšha"

#: CancelBtn
