#: 1
msgid "out of memory"
msgstr "lahlegetšwe ke tlhaologanyo"

#: 2
msgid "syntax error"
msgstr "phošo ya popafoko"

#: 3
msgid "no element found"
msgstr "ga go karolo yeo e hweditšwego"

#: 4
msgid "not well-formed"
msgstr "ga se ya hlangwa gabotse"

#: 5
msgid "unclosed token"
msgstr "sešupo se sa tsebjego"

#: 6
msgid "partial character"
msgstr "tlhaka ya go hlaola"

#: 7
msgid "mismatched tag"
msgstr "theke yeo e sa swanelago"

#: 8
msgid "duplicate attribute"
msgstr "tlhaolo ya khopi"

#: 9
msgid "junk after document element"
msgstr "lahla ka morago ga karolo ya tokumente"

#: 10
msgid "illegal parameter entity reference"
msgstr "tšhupetšo ya selo sa pharameta se e sego sa semolao"

#: 11
msgid "undefined entity"
msgstr "selo se sa hlaloswego"

#: 12
msgid "recursive entity reference"
msgstr "tšhupetšo ya selo se diragalago gape"

#: 13
msgid "asynchronous entity"
msgstr "selo se sa diregego ka nako e tee"

#: 14
msgid "reference to invalid character number"
msgstr "tšhupetšo go nomoro ya tlhaka ye e sego ya makgonthe"

#: 15
msgid "reference to binary entity"
msgstr "tšhupetšo go selo sa karolo tše pedi"

#: 16
msgid "reference to external entity in attribute"
msgstr "tšhupetšo go selo sa ka ntle go tlhaolo"

#: 17
msgid "xml declaration not at start of external entity"
msgstr "kgoeletšo ya xml ga e go mathomo a selo sa ka ntle"

#: 18
msgid "unknown encoding"
msgstr "phetošo ye e sa tsebjego"

#: 19
msgid "encoding specified in XML declaration is incorrect"
msgstr "phetošo yeo e laeditšwego go kgoeletšo ya XML ga se ya maleba"

#: 20
msgid "unclosed CDATA section"
msgstr "karolo ya CDATA ye e sa utullwago"

#: 21
msgid "error in processing external entity reference"
msgstr "phošo go sepetšeng tšhupetšo ya selo sa ka ntle"

#: 22
msgid "document is not standalone"
msgstr "tokumente ga se tšhikanoši"

#: 23
msgid "unexpected parser state"
msgstr "seemo sa sefetleki seo se sa letelwago"

#: 24
msgid "entity declared in parameter entity"
msgstr "selo seo se goeleditšwego go selo sa pharametha"

#: 27
msgid "prefix not bound to a namespace"
msgstr "hlogo ga se ya ithekga ka sekgobaina"

#: 29
msgid "reserved prefix (xml) must not be undeclared or bound to another namespace URI"
msgstr "hlogo yeo e beetšwego thoko (xml) ga ya swanela go se goeletšwe goba go ithekga ka sekgobaina se sengwe sa URI"

#: 30
msgid "reserved prefix (xmlns) must not be declared or undeclared"
msgstr "hlogo yeo e beetšwego thoko (xmlns) ga ya swanela go goeletšwa goba go se goeletšwe"

#: 31
msgid "prefix must not be bound to one of the reserved namespace URIs"
msgstr "hlogo ga ya swanela go ithekga go ye nngwe ya dikgobaina tše di beetšwego thoko tša di-URI"

#: XMLParsingError
msgid "XML Parsing Error: %1$S\n"
"Location: %2$S\n"
"Line Number %3$d, Column %4$d:"
msgstr "XML Phošo ya Tlhophollantšu: %1$S\nLefelo: %2$S\nNomoro ya Mothaladi %3$d, Kholomo %4$d:"

#: Expected
msgid ". Expected: </%S>."
msgstr ". Letetšwego: </%S>."

