msgid "Extensions (*.xpi)"
msgstr "Dikoketšo (*.xpi)"

#: themesFilter
msgid "Themes (*.jar)"
msgstr "Dihlogo (*.jar)"

#: installThemePickerTitle
msgid "Select a theme to install"
msgstr "Kgetha sehlogo go se lokela"

#: installExtensionPickerTitle
msgid "Select an extension to install"
msgstr "Kgetha koketšo go e lokela"

#: dssSwitchAfterRestart
msgid "Restart %S to use."
msgstr "Thoma %S gape go diriša."

#: finishedUpdateCheck
msgid "Finished checking for updates to %S"
msgstr "E feditše go lekola dikaonafatšo tša %S"

#: updateAvailableMsg
msgid "Version %S is available."
msgstr "Tšweletšo %S e a hwetšagala."

#: xpinstallDisabledMsgLocked
msgid "Software installation has been disabled by your system administrator."
msgstr "Go lokelwa sofetewere go šitišitšwe ke molaodi wa tshepedišo ya gago."

#: xpinstallDisabledMsg
msgid ""
"Software installation is currently disabled. Click Enable and try again."
msgstr ""
"Go lokelwa ga sofetewere ga bjale go šitišitšwe. Kgotla Kgonagatša gomme o "
"leke gape."

