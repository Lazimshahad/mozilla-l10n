#: tabsTab.label
msgid "Tabs"
msgstr "Li-tab"

#: openExternalLinksIn.label
msgid "Open links from other applications in:"
msgstr "Bula mahokela ho tsoa litšebetsong tse ling ho:"

#: newWindow.label
msgid "a new window"
msgstr "fenstere e ncha"

#: newTabInRecentWindow.label
msgid "a new tab in the most recent window"
msgstr "tab e ncha fenstereng ea moraorao ka ho fetisa"

#: recentWindowOrTab.label
msgid "the most recent tab/window"
msgstr "fenstere/tab ea moraorao ka ho fetisa"

#: forceNewWindowLinks.label
msgid "Force links that open new windows to open in:"
msgstr "E tlamella mahokela a bulang lifenstere tse ncha ho:"

#: sameTabOrWindow.label
msgid "the same tab/window as the link"
msgstr "fenstere/tab e tšoanang le lehokela"

#: newTab.label
msgid "a new tab"
msgstr "tab e ncha"

#: hideTabBar.label
msgid "Hide the tab bar when only one web site is open"
msgstr "Pata mola oa tab ha setša sa inthanete se le seng feela se butsoe"

#: loadInBackground.label
msgid "Select new tabs opened from links"
msgstr "Khetha li-tab tse ncha tse butsoeng mahokeleng"

#: loadBookmarksInBackground.label
msgid "Select new tabs opened from bookmarks or history"
msgstr "Khetha li-tab tse ncha tse butsoeng litšoaeabukeng kapa historing"

#: warnOnClose.label
msgid "Warn when closing multiple tabs"
msgstr "Lemosa ha e koala li-tab tse ngata"
