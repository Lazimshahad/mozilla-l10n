#: 1
msgid "out of memory"
msgstr "e feletsoe ke memori"

#: 2
msgid "syntax error"
msgstr "phoso ea sebopeho-puo"

#: 3
msgid "no element found"
msgstr "ha ho elemente e fumanoeng"

#: 4
msgid "not well-formed"
msgstr "ha e a bopeha hantle"

#: 5
msgid "unclosed token"
msgstr "thokene e sa koaloang"

#: 6
msgid "partial character"
msgstr "tlhaku e sa fellang"

#: 7
msgid "mismatched tag"
msgstr "tag e fapantsoeng"

#: 8
msgid "duplicate attribute"
msgstr "popeho ea luplikeiti"

#: 9
msgid "junk after document element"
msgstr "matlakala kamora elemente ea tokomane"

#: 10
msgid "illegal parameter entity reference"
msgstr "referense ea pharamita ha e molaong"

#: 11
msgid "undefined entity"
msgstr "ntho e sa hlalosoang"

#: 12
msgid "recursive entity reference"
msgstr "referense ea 'recursive' bakeng sa ntho"

#: 13
msgid "asynchronous entity"
msgstr "ntho e hlaheng ka nako e le 'ngoe"

#: 14
msgid "reference to invalid character number"
msgstr "referense bakeng sa nomoro ea tlhaku e fosahetseng"

#: 15
msgid "reference to binary entity"
msgstr "referense bakeng sa ntho e habeli (binary)"

#: 16
msgid "reference to external entity in attribute"
msgstr "referense bakeng sa ntho e kantle ka popeho"

#: 17
msgid "xml declaration not at start of external entity"
msgstr "phatlalatso ea xml ha e qalehong ea ntho"

#: 18
msgid "unknown encoding"
msgstr "ho k'houdoa ho sa tsejoeng"

#: 19
msgid "encoding specified in XML declaration is incorrect"
msgstr "ho k'houdoa ho hlalositsoeng ka phatlalatso ea XML ho fosahetse"

#: 20
msgid "unclosed CDATA section"
msgstr "karolo ea CDATA e sa koaloang"

#: 21
msgid "error in processing external entity reference"
msgstr "phoso e hlahile ho sebetsoeng ha referense ea ntho e kantle"

#: 22
msgid "document is not standalone"
msgstr "tokomane ha se e ikemetseng"

#: 23
msgid "unexpected parser state"
msgstr "boemo ba 'sebopeho-puo' bo sa lebelloang"

#: 24
msgid "entity declared in parameter entity"
msgstr "ntho e phatlalalitsoe ka paramitha ea ntho"

#: 27
msgid "prefix not bound to a namespace"
msgstr "ketapele ha e a matahana le sebakalebitso"

#: 29
msgid "reserved prefix (xml) must not be undeclared or bound to another namespace URI"
msgstr ""
"ketapele e behelelitsoeng (xml) ha e a lokela ho phatlalatsolloa kapa ho "
"matahangoa ho lebitsosebaka URI le leng"

#: 30
msgid "reserved prefix (xmlns) must not be declared or undeclared"
msgstr ""
"ketapele e behelelitsoeng (xmlns) ha e a lokela ho phatlalatsoa kapa ho "
"phatlalatsolloa"

#: 31
msgid "prefix must not be bound to one of the reserved namespace URIs"
msgstr ""
"ketapele ha e a lokela ho mahangoa le le leng la lebitsosebaka URI le leng "
"la a behelelitsoeng"

#: XMLParsingError
msgid "XML Parsing Error: %1$S\n"
"Location: %2$S\n"
"Line Number %3$d, Column %4$d:"
msgstr ""
"XML Phoso ea sebopeho-puo: %1$S\n"
"Tulo: %2$S\n"
"Nomoro ea Laene %3$d, K'Holomo %4$d:"

#: Expected
msgid ". Expected: </%S>."
msgstr ". E lebeletsoeng: </%S>."
