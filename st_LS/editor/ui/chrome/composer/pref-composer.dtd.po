msgstr "metsotso"

#: exterLegend.label
msgid "External Editors"
msgstr "Bahlophisi ba Kantle"

#: htmlSource
msgid "HTML Source:"
msgstr "Mohloli oa HTML:"

#: imageeditor
msgid "Images:"
msgstr "Litšoantšo:"

#: chooseButton.label
msgid "Choose"
msgstr "Khetha"

#: recentFiles.title
msgid "Recent Pages Menu"
msgstr "Menu ea Maqephe a Moraorao"

#: documentsInMenu.label
#: documentsInMenu.accesskey
msgid "Maximum &number of pages listed:"
msgstr "Palo e kaholimolimo ea &maqephe a fanoeng:"

#: savingFiles.title
msgid "When Saving or Publishing Pages"
msgstr "Ha e Boloka kapa e Phatlalatsa Maqephe"

#: preserveExisting.label
#: preserveExisting.accesskey
msgid "&Preserve original source formatting"
msgstr "&Sireletsa mohloli oa motheo oa ho fomata"

#: preserveExisting.tooltip
msgid "Preserves line breaks and page's original formatting"
msgstr "E sireletsa mapomelo a mela le ho fomatoa ha leqephe ha motheo"

#: saveAssociatedFiles.label
#: saveAssociatedFiles.accesskey
msgid "&Save images and other associated files when saving pages"
msgstr ""
"&E boloka litšoantšo le lifaele tse ling tse amanang le tsona ha e boloka "
"maqephe"

#: showPublishDialog.label
#: showPublishDialog.accesskey
msgid "&Always show Publish dialog when publishing pages"
msgstr "&Kamehla bontša Phatlalatso ea puisano ha o phatlalatsa maqephe"

#: composerEditing.label
msgid "Editing"
msgstr "Ho elita"

#: maintainStructure.label
#: maintainStructure.accesskey
msgid "&Maintain table layout when inserting or deleting cells"
msgstr "&Tsoeletsa pele moralo oa tafole ha o kenea kapa o phumula lisele"

#: maintainStructure.tooltip
msgid "Preserves table's rectangular shape by automatically adding cells after inserting or deleting cells"
msgstr ""
"Sireletsa sebopeho sa rekthenkele sa tafole ka ho eketsa lisele ka tsela e "
"iketsang kamora ho kenea kapa ho phumula lisele"

#: useCSS.label
#: useCSS.accesskey
msgid "&Use CSS styles instead of HTML elements and attributes"
msgstr "&Sebelisa litaele tsa CSS ho fapana le lielemente tsa HTML le lipopeho"

#: crInPCreatesNewP.label
#: crInPCreatesNewP.accesskey
msgid "&Return in a paragraph always creates a new paragraph"
msgstr "&Ho tobetsa konopo ea Khutla serapeng kamehla ho etsa serapa se secha"
