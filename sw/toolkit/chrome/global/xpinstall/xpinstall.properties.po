msgid "Executing: %s with argument: %s"
msgstr "Kutekeleza: %s kwa hoja: %s"

#: CopyFile
msgid "Copy File: %s to %s"
msgstr "Nakili Faili: %s hadi %s"

#: ExecuteFile
msgid "Execute File: %s"
msgstr "Tekeleza Faili: %s"

#: ExecuteFileWithArgs
msgid "Execute File: %s with argument: %s"
msgstr "Tekeleza Faili: %s kwa hoja: %s"

#: MoveFile
msgid "Move File: %s to %s"
msgstr "Hamisha Faili: %s hadi %s"

#: RenameFile
msgid "Rename File: %s to %s"
msgstr "Ita jina tena Faili: %s kuwa %s"

#: CreateFolder
msgid "Create Folder: %s"
msgstr "Unda Kabrasha: %s"

#: RemoveFolder
msgid "Remove Folder: %s"
msgstr "Ondoa Kabrasha: %s"

#: RenameFolder
msgid "Rename Folder: %s to %s"
msgstr "Ita jina tena Kabrasha: %s kuwa %s"

#: WindowsShortcut
msgid "Windows Shortcut: %s"
msgstr "Njia mkato ya Windows: %s"

#: MacAlias
msgid "Mac Alias: %s"
msgstr "Mac Alias: %s"

#: WindowsRegisterServer
msgid "Windows Register Server: %s"
msgstr "Sava ya Kisajili cha Windows: %s"

#: UnknownFileOpCommand
msgid "Unknown file operation command!"
msgstr "Amri ya uendeshaji faili isiyojulikana!"

#: Patch
msgid "Patching: %s"
msgstr "Kuziba: %s"

#: Uninstall
msgid "Uninstalling: %s"
msgstr "Kusakinusha: %s"

#: RegSkin
msgid "Register Skin: %s"
msgstr "Andikisha Ngozi: %s"

#: RegLocale
msgid "Register Locale: %s"
msgstr "Andikisha Mandhari: %s"

#: RegContent
msgid "Register Content: %s"
msgstr "Sajili Yallyomo: %s"

#: RegPackage
msgid "Register Package: %s"
msgstr "Sajili Furushi: %s"

#. #--------------------------------------------------------------------
#. #   Dialog Messages
#. #--------------------------------------------------------------------
#: ApplyNowSkin
msgid "Use this theme"
msgstr "Tumia mandhari haya"

#: ApplyNowLocale
msgid "Use this locale"
msgstr "Tumia mandhari haya"

#: ConfirmSkin
msgid "Install the theme \"%1$S\" from %2$S?"
msgstr "Sakinisha mandhari \"%1$S\" kutoka %2$S?"

#: ConfirmLocale
msgid "Install the locale \"%1$S\" from %2$S?"
msgstr "Sakinisha mandhari \"%1$S\" kutoka %2$S?"

#: OK
msgid "Install"
msgstr "Sakinisha"

#: progress.queued
msgid "Queued"
msgstr "Imefolenishwa"

#: progress.downloading
msgid "Downloading…"
msgstr "Kupakua..."

#: progress.downloaded
msgid "Downloaded"
msgstr "Imepakuliwa"

#: progress.installing
msgid "Installing…"
msgstr "Kusakinisha..."

#: Unsigned
msgid "Unsigned"
msgstr "Isiyo na sahihi"

#. #--------------------------------------------------------------------
#. #   Miscellaneous
#. #--------------------------------------------------------------------
#: ERROR
msgid "ERROR"
msgstr "KOSA"

#: error0
msgid "Success"
msgstr "Mafanikio"

#: error999
msgid "Restart to complete"
msgstr "Anzisha tena kukamilisha"

#: error-202
msgid "Access denied"
msgstr "Ufikivu umekatazwa"

#: error-203
msgid ""
"Unexpected installation error\n"
"Review the Error Console log for more details."
msgstr ""
"Kosa la kusakinisha ambalo halikutarajiwa\n"
"Angalia tena Konsoli ya Kosa kwa maelezo zaidi."

#: error-204
msgid "Install script not found"
msgstr "Hati ya kusakinisha haikupatikana"

#: error-207
msgid "Not a valid install package"
msgstr "Furushi la kusakinisha lisilo sahihi"

#: error-208
msgid "Invalid argument"
msgstr "Hoja batili"

#: error-210
msgid "User canceled"
msgstr "Mtumiaji amekatizwa"

#: error-214
msgid "Required file does not exist"
msgstr "Faili inayohitajika haipo"

#: error-215
msgid "Read only"
msgstr "Soma pekee"

#: error-218
msgid "AppleSingle extraction error"
msgstr "Kosa la kufyonza la AppleSingle"

#: error-219
msgid "Invalid path"
msgstr "Njia batili"

#: error-225
msgid "EXTRACTION_FAILED"
msgstr "KUFYONZA_KUMESHINDWA"

#: error-227
msgid "Canceled"
msgstr "Imekatizwa"

#: error-228
msgid "Download error"
msgstr "Kosa la kupakua"

#: error-229
msgid "Script error"
msgstr "Kosa la hati"

#: error-230
msgid "Already exists"
msgstr "Tayari ipo"

#: error-235
msgid "Out of space"
msgstr "Nafasi imemalizika"

#: error-239
msgid "Chrome registration failed"
msgstr "Usajili wa kromu umeshindwa"

#: error-240
msgid "Unfinished install"
msgstr "Usakinishaji haukukamilika"

#: error-244
msgid "Unsupported package"
msgstr "Furushi lisiloauniwa"

#: error-260
msgid "Signing could not be verified."
msgstr "Kuweka saini hakungedhibitishwa."

#: error-261
msgid "Invalid file hash (possible download corruption)"
msgstr "Reli ya faili batili (uwezekano wa kuvurugwa kwa kipakuzi)"

#: error-262
msgid "Unknown or invalid file hash type"
msgstr "Aina ya reli isiyotambulika au batili"

#: error-299
msgid "Out of memory"
msgstr "Kumbukumbu imemalizika"

