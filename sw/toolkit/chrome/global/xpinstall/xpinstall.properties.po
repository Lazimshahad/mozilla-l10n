msgid "Deleting file: %s"
msgstr "Kufuta faili: %s"

#: DeleteComponent
msgid "Deleting component: %s"
msgstr "Kufuta kijenzi: %s"

#: Execute
msgid "Executing: %s"
msgstr "Kutekeleza: %s"

