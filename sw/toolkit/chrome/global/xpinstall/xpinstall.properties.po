#. #--------------------------------------------------------------------
#. #   Install Actions
#. #--------------------------------------------------------------------
#: InstallFile
msgid "Installing: %s"
msgstr "Kusakinisha:%s"

#: InstallSharedFile
msgid "Installing Shared File: %s"
msgstr "Kusakinisha Faili Gawize: %s"

#: ReplaceFile
msgid "Replacing: %s"
msgstr "Kubadililsha: %s"

#: ReplaceSharedFile
msgid "Replacing Shared File: %s"
msgstr "Kubadilisha Faili Gawize: %s"

#: SkipFile
msgid "Skipping: %s"
msgstr "Kuruka: %s"

#: SkipSharedFile
msgid "Skipping Shared File: %s"
msgstr "Kuruka Faili Gawize: %s"

