#. # Main toolbar buttons (tooltips and alt text for images)
#: previous.title
msgid "Previous Page"
msgstr "முந்தைய பக்கம்"

#: previous_label
msgid "Previous"
msgstr "முந்தைய"

#: next.title
msgid "Next Page"
msgstr "அடுத்த பக்கம்"

#: next_label
msgid "Next"
msgstr "அடுத்தது"

#. # LOCALIZATION NOTE (page_label, page_of):
#. # These strings are concatenated to form the "Page: X of Y" string.
#. # Do not translate "{{pageCount}}", it will be substituted with a number
#. # representing the total number of pages.
#: page_label
msgid "Page:"
msgstr "பக்கம்:"

#: page_of
msgid "of {{pageCount}}"
msgstr "மொத்தம் {{pageCount}} இல்"

#: zoom_out.title
msgctxt "zoom_out.title"
msgid "Zoom Out"
msgstr "சிறியதாக்கு"

#: zoom_out_label
msgctxt "zoom_out_label"
msgid "Zoom Out"
msgstr "சிறியதாக்கு"

#: zoom_in.title
msgctxt "zoom_in.title"
msgid "Zoom In"
msgstr "பெரிதாக்கு"

#: zoom_in_label
msgctxt "zoom_in_label"
msgid "Zoom In"
msgstr "பெரிதாக்கு"

#: zoom.title
msgid "Zoom"
msgstr "பெரிதாக்கு"

#: print.title
msgctxt "print.title"
msgid "Print"
msgstr "அச்சிடு"

#: print_label
msgctxt "print_label"
msgid "Print"
msgstr "அச்சிடு"

#: open_file.title
msgid "Open File"
msgstr "கோப்பைத் திற"

#: open_file_label
msgid "Open"
msgstr "திற"

#: download.title
msgctxt "download.title"
msgid "Download"
msgstr "பதிவிறக்கு"

#: download_label
msgctxt "download_label"
msgid "Download"
msgstr "பதிவிறக்கு"

#: bookmark.title
msgid "Current view (copy or open in new window)"
msgstr "தற்போதைய காட்சி (நகலெடு அல்லது புதிய சாளரத்தில் திற)"

#: bookmark_label
msgid "Current View"
msgstr "தற்போதைய காட்சி"

#. # Tooltips and alt text for side panel toolbar buttons
#. # (the _label strings are alt text for the buttons, the .title strings are
#. # tooltips)
#: toggle_slider.title
msgctxt "toggle_slider.title"
msgid "Toggle Slider"
msgstr "ஸ்லைடரை நிலைமாற்று"

#: toggle_slider_label
msgctxt "toggle_slider_label"
msgid "Toggle Slider"
msgstr "ஸ்லைடரை நிலைமாற்று"

#: outline.title
msgid "Show Document Outline"
msgstr "ஆவணத்தின் வெளிவரையைக் காண்பி"

#: outline_label
msgid "Document Outline"
msgstr "ஆவணத்தின் வெளிவரை"

#: thumbs.title
msgid "Show Thumbnails"
msgstr "சிறுபடத்தைக் காண்பி"

#: thumbs_label
msgid "Thumbnails"
msgstr "சிறுபடம்"

#: search_panel.title
msgid "Search Document"
msgstr "ஆவணத்தைத் தேடு"

#: search_panel_label
msgid "Search"
msgstr "தேடு"

#. # Document outline messages
#: no_outline
msgid "No Outline Available"
msgstr "வெளிவரை இல்லை"

#. # Thumbnails panel item (tooltip and alt text for images)
#. # LOCALIZATION NOTE (thumb_page_title): "{{page}}" will be replaced by the page
#. # number.
#: thumb_page_title
msgid "Page {{page}}"
msgstr "பக்கம் {{page}}"

#. # LOCALIZATION NOTE (thumb_page_canvas): "{{page}}" will be replaced by the page
#. # number.
#: thumb_page_canvas
msgid "Thumbnail of Page {{page}}"
msgstr "பக்கத்தின் சிறுபடம் {{page}}"

#. # Search panel button title and messages
#: search
msgid "Find"
msgstr "கண்டறி"

#: search_terms_not_found
msgid "(Not found)"
msgstr "(இல்லை)"

#. # Error panel labels
#: error_more_info
msgid "More Information"
msgstr "அதிக தகவல்"

#: error_less_info
msgid "Less Information"
msgstr "குறைவான தகவல்"

#: error_close
msgid "Close"
msgstr "மூடு"

#. # LOCALIZATION NOTE (error_build): "{{build}}" will be replaced by the PDF.JS
#. # build ID.
#: error_build
msgid "PDF.JS Build: {{build}}"
msgstr "PDF.JS பில்டு: {{build}}"

#. # LOCALIZATION NOTE (error_message): "{{message}}" will be replaced by an
#. # english string describing the error.
#: error_message
msgid "Message: {{message}}"
msgstr "செய்தி: {{message}}"

#. # LOCALIZATION NOTE (error_stack): "{{stack}}" will be replaced with a stack
#. # trace.
#: error_stack
msgid "Stack: {{stack}}"
msgstr "தொகுப்பு: {{stack}}"

#. # LOCALIZATION NOTE (error_file): "{{file}}" will be replaced with a filename
#: error_file
msgid "File: {{file}}"
msgstr "கோப்பு: {{file}}"

#. # LOCALIZATION NOTE (error_line): "{{line}}" will be replaced with a line number
#: error_line
msgid "Line: {{line}}"
msgstr "வரி: {{line}}"

#: rendering_error
msgid "An error occurred while rendering the page."
msgstr "பக்கத்தைக் காட்சியாக்கும் போது பிழை ஏற்பட்டது."

#. # Predefined zoom values
#: page_scale_width
msgid "Page Width"
msgstr "பக்க அகலம்"

#: page_scale_fit
msgid "Page Fit"
msgstr "பக்கப் பொருத்தம்"

#: page_scale_auto
msgid "Automatic Zoom"
msgstr "தானியக்க பெரிதாக்கம்"

#: page_scale_actual
msgid "Actual Size"
msgstr "உண்மையான அளவு"

#. # Loading indicator messages
#: loading_error_indicator
msgid "Error"
msgstr "பிழை"

#: loading_error
msgid "An error occurred while loading the PDF."
msgstr "PDF ஐ ஏற்றும் போது ஒரு பிழை ஏற்பட்டது."

#. # LOCALIZATION NOTE (text_annotation_type): This is used as a tooltip.
#. # "{{type}}" will be replaced with an annotation type from a list defined in
#. # the PDF spec (32000-1:2008 Table 169 – Annotation types).
#. # Some common types are e.g.: "Check", "Text", "Comment", "Note"
#: text_annotation_type
msgid "[{{type}} Annotation]"
msgstr "[{{type}} சுருக்க உரை]"

#: request_password
msgid "PDF is protected by a password:"
msgstr "PDF கடவுச்சொல்லால் பாதுகாக்கப்பட்டது:"

#: printing_not_supported
msgid "Warning: Printing is not fully supported by this browser."
msgstr "எச்சரிக்கை: இந்த உலாவி அச்சிடுவதற்கு முழுமையான ஆதரவு கொண்டதல்ல."
