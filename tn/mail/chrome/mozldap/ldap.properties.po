#: authPromptTitle
msgid "LDAP Server Password Required"
msgstr "Go tlhokega Khunololamoraba ya Dikgatlhong tsa LDAP"

#: authPromptText
msgid "Please enter your password for %1$S."
msgstr "Tsweetswee tsenya khunololamoraba ya gago ya %1$S."

#: 1
msgid "Operations error"
msgstr "Phoso ya tiro"

#: 2
msgid "Protocol error"
msgstr "Phoso ya porotokolo"

#: 3
msgid "Timelimit exceeded"
msgstr "Go fetilwe nako e e lekanyeditsweng"

#: 4
msgid "Sizelimit exceeded"
msgstr "Go fetilwe bogolo jo bo lekanyeditsweng"

#: 5
msgid "Compare false"
msgstr "Tshwantshanyo ga se ya nnete"

#: 6
msgid "Compare true"
msgstr "Tshwantshanyo ke ya nnete"

#: 7
msgid "Authentication method not supported"
msgstr "Mokgwa wa rurifatso ga o a tshegediwa"

#: 8
msgid "Strong authentication required"
msgstr "Go tlhokega thurifatso e e maatla"

#: 9
msgid "Partial results and referral received"
msgstr "Go amogetswe dipholo tse di sa felelang le phetiso"

#: 10
msgid "Referral received"
msgstr "Go amogetswe phetiso"

#: 11
msgid "Administrative limit exceeded"
msgstr "Go fetilwe tekanyetso ya tsamaiso"

#: 12
msgid "Unavailable critical extension"
msgstr "Katololoso ya botlhokwa e e seyong"

#: 13
msgid "Confidentiality required"
msgstr "Go tlhokega bosephiri"

