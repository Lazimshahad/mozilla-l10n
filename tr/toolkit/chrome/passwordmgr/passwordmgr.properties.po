#: rememberValue
msgid "Use Password Manager to remember this value."
msgstr "Bu değeri kaydetmek için parola yöneticisini kullan."

#: rememberPassword
msgid "Use Password Manager to remember this password."
msgstr "Bu parolayı hatırlamak için parola yöneticisini kullan."

#: savePasswordTitle
msgid "Confirm"
msgstr "Onayla"

#. # LOCALIZATION NOTE (rememberPasswordText):
#. # 1st string is the username for the login, 2nd is the login's hostname. 
#. # Note that long usernames may be truncated.
#: rememberPasswordText
msgid "Remember password for \"%1$S\" on %2$S?"
msgstr "%2$S konumundaki \"%1$S\" kullanıcı adı ve parolası hatırlansın mı?"

#. # LOCALIZATION NOTE (rememberPasswordTextNoUsername):
#. # String is the login's hostname.
#: rememberPasswordTextNoUsername
msgid "Remember password on %S?"
msgstr "%S konumundaki parolalar hatırlansın mı?"

#: notNowButtonText
msgid "&Not Now"
msgstr "Şi&mdi değil"

#: notifyBarNotNowButtonText
msgid "Not Now"
msgstr "Şimdi değil"

#: notifyBarNotNowButtonAccessKey
msgid "N"
msgstr "m"

#: neverForSiteButtonText
msgid "Ne&ver for This Site"
msgstr "&Bu sayfada asla"

#: notifyBarNeverRememberButtonText
msgid "Never Remember Password for This Site"
msgstr "Bu sitedeki parolaları asla hatırlama"

#: notifyBarNeverRememberButtonAccessKey
msgid "e"
msgstr "e"

#: rememberButtonText
msgid "&Remember"
msgstr "&Hatırla"

#: notifyBarRememberPasswordButtonText
msgid "Remember Password"
msgstr "Parolayı hatırla"

#: notifyBarRememberPasswordButtonAccessKey
msgid "R"
msgstr "R"

#: passwordChangeTitle
msgid "Confirm Password Change"
msgstr "Parola değişimini onaylama"

#. # LOCALIZATION NOTE (updatePasswordText):
#. # String is the username for the login.
#: updatePasswordText
msgid "Update saved password for \"%S\"?"
msgstr "\"%S\" ile ilişkili parolalar güncellensin mi?"

#: updatePasswordTextNoUser
msgid "Update saved password?"
msgstr "Kayıtlı parolalar güncellensin mi?"

#: notifyBarUpdateButtonText
msgid "Update Password"
msgstr "Parolayı güncelle"

#: notifyBarUpdateButtonAccessKey
msgid "U"
msgstr "o"

#: notifyBarDontChangeButtonText
msgid "Don't Change"
msgstr "Değiştirme"

#: notifyBarDontChangeButtonAccessKey
msgid "D"
msgstr "m"

#: userSelectText
msgid "Please confirm which user you are changing the password for"
msgstr "Hangi kullanıcı için parola değiştirmek istediğiniz lütfen onaylayın."

#: hidePasswords
msgid "Hide Passwords"
msgstr "Parolaları gizle"

#: hidePasswordsAccessKey
