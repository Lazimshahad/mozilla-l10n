#: hostColon
msgid "Host:"
msgstr "میزبان:"

#: domainColon
msgid "Domain:"
msgstr "عمل داری:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "فقط خفیہ کردہ کنکشن"

#: forAnyConnection
msgid "Any type of connection"
msgstr "کسی بھی قسم کا کنکشن"

#: expireAtEndOfSession
#, fuzzy
msgid "At end of session"
msgstr "سیشن کے اختتام پر"

#: showDetails
msgid "Show Details"
msgstr "تفصیلات نمائش کریں"

#: hideDetails
msgid "Hide Details"
msgstr "تفصیلات چھپائیں"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "سائٹ %S ایک  کوکی سیٹ کرنا چاہتی ہے۔"

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "سائٹ %S دوسری  کوکی سیٹ کرنا چاہتی ہے۔"

#. #  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"سائٹ %S ایک اور کوکی سیٹ کرنا چاہتی ہے۔ آپ پہلے ہی اس سائٹ سے %S کوکیاں "
"رکھتے ہیں۔ "

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "سائٹ %S ایک موجود کوکی ترمیم کرنا چاہتی ہے۔"

#~ msgid "The site %S wants to load an image."
#~ msgstr "سائٹ %S ایک نقش لوڈ کرنا چاہتی ہے۔"

#~ msgid "Remember this decision"
#~ msgstr "یہ فیصلہ یاد رکھیں"
