#: safeModeDialog.title
msgid "&brandShortName; Safe Mode"
msgstr "I&brandShortName; Indawo Elondekile"

#: window.width
msgid "_: Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger.\n"
"37em"
msgstr "37em"

#: safeModeDescription.label
msgid "&brandShortName; is now running in Safe Mode, which temporarily disables your custom settings, themes, and extensions."
msgstr "I&brandShortName; manje isebenza eSimweni Esilondekile, eyenza okwesikhashana ukuhlelwa kwezinto ezivamile, izindikimba nezithasiselo zamagama kungasebenzi."

#: safeModeDescription2.label
msgid "You can make some or all of these changes permanent:"
msgstr "Ungenza ukuba olunye lwalushintsho noma lonke luhlale njalo:"

#: disableThemes.label
#: disableThemes.accesskey
msgid "&Disable all themes and extensions"
msgstr "&Yenza zonke izindikimba nezithasiselo zamagama kungasebenzi"

#: resetToolbars.label
#: resetToolbars.accesskey
msgid "&Reset toolbars and controls"
msgstr "&Hlela kabusha ama-toolbar nezinto zokulawula"

#: resetBookmarks.label
#: resetBookmarks.accesskey
msgid "Reset &bookmarks to &brandShortName; defaults"
msgstr "Hlela kabusha &izimpawu zokubekisa zibe i-&brandShortName; kwenze ngendlela evamile"

#: resetUserPrefs.label
#: resetUserPrefs.accesskey
msgid "Reset all user &preferences to &brandShortName; defaults"
msgstr "Hlela kabusha &konke okukhethwa ku &brandShortName; kwenzeke ngendlela evamile njalo"

#: changeAndRestartButton.label
msgid "Make Changes and Restart"
msgstr "Yenza Ushintsho bese Uqala Kabusha"

#: continueButton.label
msgid "Continue in Safe Mode"
msgstr "Qhubeka Esimweni Esilondekile"

