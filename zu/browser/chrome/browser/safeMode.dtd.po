#~ msgid "&Reset toolbars and controls"
#~ msgstr "&Hlela kabusha ama-toolbar nezinto zokulawula"

#~ msgid "Delete all &bookmarks except for backups"
#~ msgstr "Susa konke &uphawu lokubekisa ngaphandle kwalokho okukophiwe"

#~ msgid "Reset all user &preferences to &brandShortName; defaults"
#~ msgstr ""
#~ "Hlela kabusha &konke okukhethwa ku &brandShortName; kwenzeke ngendlela "
