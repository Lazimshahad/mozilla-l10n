#: error.login.reason.network
msgid "Failed to connect to the server"
msgstr "Kuhlelekile ukuba kuxhumeke kwisiphakeli"

#: error.login.reason.synckey
msgid "Wrong Sync Key"
msgstr "Isikhiya esingalungile sokuhlanganisa"

#: error.login.reason.account
msgid "Incorrect account name or password"
msgstr "Igama le-akhawunti noma i-password angalungile"

#: error.login.reason.no_username
msgid "Missing account name"
msgstr ""

#: error.login.reason.no_password2
msgid "Missing password"
msgstr ""

#: error.login.reason.no_synckey
msgid "No saved Sync Key to use"
msgstr "Ayikho ikhiya yokuhlanganisa egciniwe ongayisebenzisa"

#: error.login.reason.server
msgid "Server incorrectly configured"
msgstr "Isiphakeli silungiswe ngokungalkungile"

#: error.sync.failed_partial
msgid "One or more data types could not be synced"
msgstr "Izimo zedatha eyodwa noma eziningi ezingakwazanga ukuhlanganiswa"

#: invalid-captcha
msgid "Incorrect words, try again"
msgstr "Amagama angalungile, zama futhi"

#: weak-password
msgid "Use a stronger password"
msgstr "Sebenzisa i-password enamadla"

#. # this is the fallback, if we hit an error we didn't bother to localize
#: error.reason.unknown
msgid "Unknown error"
msgstr "Iphutha Elingaziwa"

#: change.synckey.sameAsSyncKey
msgid "The new Sync Key cannot be the same as your Sync Key"
msgstr ""
"Ikhiya entsha yokuhlanganisa ayikwazi ukufana naleyo khiya yokuhlanganisa "
"engeyakho"

#: change.synckey.sameAsPassword
msgid "The Sync Key cannot be the same as your password"
msgstr "Ikhiya yokuhlanganisa ayikwazi ukufana ne-password yakho"

#: change.synckey.sameAsUsername
msgid "The Sync Key cannot be the same as your user name"
msgstr "Ikhiya yokuhlanganisa ayikwazi ukufana negama lakho lomsebenzisi"

#: change.synckey.sameAsEmail
msgid "The Sync Key cannot be the same as your email address"
msgstr "Ikhiya yokuhlanganisa ayikwazi ukufana nekheli lakho le-imeyili"

#: change.synckey.tooShort
msgid "The Sync Key entered is too short"
msgstr "Ikhiya lokuhlanganisa olifakile lifishane kakhulu"

#: change.password.pwSameAsSyncKey
msgid "Password can't match your Sync Key"
msgstr "I-password ayikwazi ukfana nekhiya yakho yokuhlanganisa"

#: change.password.pwSameAsPassword
msgid "Password can't match current password"
msgstr "I-password ayikwazi ukufana ne-password yamanje"

#: change.password.pwSameAsUsername
msgid "Password can't match your user name"
msgstr "I-password ayikwazi ukufana negama lakho lomsebenzisi"

#: change.password.pwSameAsEmail
msgid "Password can't match your email address"
msgstr "I-password ayikwazi ukufana nekheli lakho le-imeyili"

#: change.password.mismatch
msgid "The passwords entered do not match"
msgstr "Ama-password owafakile awafani"

#: change.password.tooShort
msgid "The password entered is too short"
msgstr "I-password oyifakile imfishane kakhulu"

#~ msgid "No saved password to use"
#~ msgstr "Ayikho i-password egciniwe ongayisebenzisa"

#~ msgid "Incorrect username or password"
#~ msgstr "Igama lomsebenzisi noma i-oassword engalungile"
